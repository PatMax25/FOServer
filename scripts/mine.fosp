	if(oldMineId>0)
	{
		Location@oldMine=GetLocation(oldMineId);
		if((@oldMine!=null))
		{
			DeleteLocation(oldMine.Id);
		}
	}
	
	Critter@[]players={player};
	uint x,y=0;
	
	if(player.GetMap().GetLocation().GetProtoId()==(32))
	{
		x=Random(130,250);
		y=Random(100,250);
	}
	else
	{
		x=Random(500,695);
		y=Random(500,672);
	}
	
	uint locId=CreateLocation((34),x,y,players);
	Location@loc=GetLocation(locId);
	loc.Color=0xFFC72E00;
	loc.Update();
	
	if(!(@loc!=null))
	{
		Log("ERR: Can't find mining location.");
		return;
	}
	Map@map=loc.GetMapByIndex(1);
	if(!(@map!=null))
	{
		Log("ERR: Can't find mining map");
		return;
	}
	
	SetRootMapData(loc,(5),uint(player.Id)); 
	
	array<Item@>spots;
	uint num=map.GetItems((21000),spots);  
	
	BanksTransferFromCritter(player,(40000),(6),false);
	SetLvar(player,(3008),loc.Id);
	SetLvar(player,(3009),int((__FullSecond)));
}
