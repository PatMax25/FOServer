	bool IsCollision(int screenX,int screenY,int mouseX,int mouseY)
	{
		if(absolutePosition)
		{
			screenX=0;
			screenY=0;
		}
		return((mouseX)>=(screenX+posX)&&(mouseX)<=(screenX+posX+elementWidth)&&(mouseY)>=(screenY+posY)&&(mouseY)<=(screenY+posY+elementHeight));
	}
	
