void picture(int p0,int p1,int p2,string@pic,array<int>@)
{
	if(!(@pic!=null)||pic.length()==0)
	return;
	
	Sprite spic;
	if(spic.Load(pic,(4)))
	{
		GUI_DeleteScreen((50));
		GUI_CreateScreen((50),pic)
		.AutoCursor(true,(6))
		.CanMove(false)
		.CloseOnMiss(false)
		.Multiinstance(false);
		GUI_ShowScreen((50),p0,p1,p2);
	}
}  

