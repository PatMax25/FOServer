uint dlg_GiveFollower(Critter&player,Critter@npc,string@say)
{
	if(!((@say!=null)&&say.length()>0))
	return 0;
	
	Critter@target=GetPlayer(say);
	if(!(@target!=null))
	{
		player.Say((12),"Invalid player.");
		return 0;
	}
	
	if(!(@npc!=null))
