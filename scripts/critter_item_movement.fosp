		if(!(@owner!=null))
		return false;
		
		array<Critter@>guards;
		uint n=follower.GetMap().GetCritters(0,(0x0F)|(0x20),guards);
		for(uint i=0;i<n;i++)
		{
			if(((guards[i].Param[(534)]&((0x00000008)))!=0)&&owner.IsSeenBy(guards[i]))
			AttackCritter(guards[i],owner);
		}
	}
	
	return false;
}

