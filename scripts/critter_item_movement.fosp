		player.Say((12),"But that's you!");
		return 0;
	}
	
	if(player.GetMapId()!=target.GetMapId())
	{
		player.Say((12),"Player is not present on this map.");
		return 0;
	}
	
	if((((target.Flags)&((0x00080000)))!=0))
