bool _Attack(Critter&follower,Critter&target)
{
	if(follower.FollowerVar[(926)]==(6))
	{
		Flee(follower);
		return true;
	}
	
	if(IsTown(follower.GetMap())&&!IsFlaggedAsIllegal(target))
	{
