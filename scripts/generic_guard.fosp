void _FactionGuard(Critter&guard,bool firstTime)
{
	critter_init(guard,firstTime);
	guard.SetEvent((4),"_OnShowCritter");
	guard.SetEvent((0),"_OnIdle");
	guard.ShowCritterDist1=(5);
	guard.SetEvent((5),"_OnShowCritter1");
}   

void _OnShowCritter(Critter&cr,Critter&target)
{
	if(GetGroupsStatus(target,cr)==(2))
	{
		AddAttackPlane(cr,0,target);
		return;
	}
	if(!(target.IsNpc()&&!(((target.Param[(534)]&((0x00000020)))!=0)))&&target.Reputation[(cr.Param[(106)])]<__ReputationHated)
	if(Random(-2000,-1500)>=target.Reputation[(cr.Param[(106)])])
	AddAttackPlane(cr,0,target);
}

void _OnShowCritter1(Critter&cr,Critter&target)
{
	
	if(target.IsPlayer()&&target.Reputation[(cr.Param[(106)])]<=-1000&&Random(1,25)==1&&IsHumanoid(cr))
	cr.Say((2),GetSafePlayerName(target.Id)+". We're watching you, scum.");
} 

void _OnIdle(Critter&cr)
{
	if(!IsHumanoid(cr))
	return;
	array<Critter@>crits;
	uint num=cr.GetCritters(false,(0x01)|(0x10),crits);
	
	for(uint i=0;i<num;i++)
	{
		if((GetCrittersDistantion(cr,crits[i])<=(5)))
		{
			if(crits[i].Reputation[(cr.Param[(106)])]>-1000)
			continue;
			
			Item@item=crits[i].GetItem(0,(1));
			if(!(@item!=null))
			continue;
			if((item.GetType()==(3))&&(Random(0,1)==0))
			{
				cr.Say((4),"Put down your weapon, NOW!");
				int[]data={crits[i].Id,cr.Id};
				CreateTimeEvent(((__FullSecond)+(((Random(4,6))*__TimeMultiplier))),"e_CheckWeapon",data,false);
			}
		}
	}
}

uint e_CheckWeapon(array<uint>@values)
{
	uint __nowZZZ=GetTick();
	Critter@player=GetCritter(values[0]);
	Critter@guard=GetCritter(values[1]);
	
	if(!(@player!=null)||!((@guard!=null)))
	{
		uint __endZZZ=GetTick();if(__endZZZ-__nowZZZ>40) Log("I am being executed for a very long time! ("+(__endZZZ-__nowZZZ)+" ms)");
		return 0;
	}
	
	Item@item=player.GetItem(0,(1));
	if(!(@item!=null))
	{
		uint __endZZZ=GetTick();if(__endZZZ-__nowZZZ>40) Log("I am being executed for a very long time! ("+(__endZZZ-__nowZZZ)+" ms)");
		return 0;
	}
	if(item.GetType()==(3)&&(player.Param[(72)]>0))
	AddAttackPlane(guard,0,player);
	uint __endZZZ=GetTick();if(__endZZZ-__nowZZZ>40) Log("I am being executed for a very long time! ("+(__endZZZ-__nowZZZ)+" ms)");
	return 0;
}
