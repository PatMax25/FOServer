	void ExecSetDeteriorationCommand(array<string@>@command,Critter&player)
	{
		int minTier=4,maxTier=4,percent=1,hours=12,multipler=1;
		bool runtime=false;
		
		array<string>min={"-min"};
		array<string>max={"-max"};
