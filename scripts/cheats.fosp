		}
		
		if(runtime)
		{
			array<uint>settings={minTier,maxTier,percent,hours};
			uint timeEvent=CreateTimeEvent(((__FullSecond)+(0)),"e_ApplyDeterioration",settings,true);
			player.Say((11),"Created loop number "+timeEvent+" for every "+hours+" hour"+(hours==1?"":"s")+", for tier "+(minTier==maxTier?""+minTier:("range "+minTier+" - "+maxTier))+" with -"+percent+"%.");
			player.Say((11),"To remove loop use following command: `erasetimeevent "+timeEvent);
		}
	}
	
	uint e_ApplyDeterioration(array<uint>@settings)
	{
		array<Item@>items;
		
		uint8 percent=settings[2];  
		
		if(settings[0]==4||settings[1]==4)
		{
			GetAllItems((721),items);
			GetAllItems((735),items);
			GetAllItems((736),items);
			GetAllItems((737),items);
			GetAllItems((3),items);
			GetAllItems((232),items);
			GetAllItems((348),items);
			GetAllItems((349),items);
			GetAllItems((394),items);
			GetAllItems((392),items);
			GetAllItems((395),items);
			GetAllItems((350),items);
			GetAllItems((352),items);
			GetAllItems((233),items);
			GetAllItems((391),items);
			GetAllItems((397),items);
			GetAllItems((25452),items);
			GetAllItems((25445),items);
			GetAllItems((25569),items);
			GetAllItems((22120),items);
			
		}
		
		uint counter=items.length();
		
		for(uint i=0;i<items.length();i++)
		{
			if(!(@items[i]!=null))
			{
				counter--;
				continue;
			}
			if(!items[i].IsDeteriorable())
			{
				counter--;
				continue;
			}
			
			if((((items[i].BrokenFlags)&((0x40)))!=0)||(((items[i].BrokenFlags)&((0x08)))!=0))
			{
				counter--;
				continue;
			}
			int maxbroken=items[i].BrokenCount;
			maxbroken=maxbroken+percent;
			(((maxbroken)>(100))?(100):(((maxbroken)<(0))?(0):(maxbroken)));
			
			SetMinWear(items[i],maxbroken);
			
			items[i].Update();
			
		}  
		
		return((settings[3])*__TimeMultiplier*3600);
	} 
	
