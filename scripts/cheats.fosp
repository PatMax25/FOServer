					string@res=GetConfigValue("config/GetAccess.cfg",sections[s],"Reserved");
					player.Say((11)," "+sections[s]+((@res!=null)?" (reserved)":""));
				}
			}
			else
			{
				player.Say((11),"Nobody in access list. What are you doing here?");
				return;
			}
		}        
		
	}
	
