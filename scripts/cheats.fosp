int dt_norm,int dr_norm,
int dt_laser,int dr_laser,
int dt_fire,int dr_fire,
int dt_plasma,int dr_plasma,
int dt_explode,int dr_explode,
int dt_electrical,int dr_electrical)
{
	crit.ParamBase[(9)]=ac;
	crit.ParamBase[(16)]=dt_norm;
