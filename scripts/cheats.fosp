	crit.ParamBase[(18)]=dt_fire;
	crit.ParamBase[(25)]=dr_fire;
	crit.ParamBase[(19)]=dt_plasma;
	crit.ParamBase[(26)]=dr_plasma;
	crit.ParamBase[(22)]=dt_explode;
	crit.ParamBase[(29)]=dr_explode;
	crit.ParamBase[(20)]=dt_electrical;
	crit.ParamBase[(27)]=dr_electrical;
}   

bool ModifyStat(Critter&player,Critter@target,const string&stat,int val)
{
	player.Say((11),"Modifying stat...");
	
	if(stat.rawLength()==3)
	{
		if(stat.rawGet(2)=='+')
		val=1;
		else if(stat.rawGet(2)=='-')
		val=-1;
		else
		return false;
		
		string sub=substring(stat,0,2);
		if(sub=="st")
		target.ParamBase[(0)]+=val;
		else if(sub=="pe")
		target.ParamBase[(1)]+=val;
		else if(sub=="en")
		target.ParamBase[(2)]+=val;
		else if(sub=="ch")
		target.ParamBase[(3)]+=val;
		else if(sub=="in")
		target.ParamBase[(4)]+=val;
		else if(sub=="ag")
		target.ParamBase[(5)]+=val;
		else if(sub=="lk")
		target.ParamBase[(6)]+=val;
		else
		{
			player.Say((11),"Wrong stat");
			return false;
		}
		
		if(val==1)
		player.Say((11),"Incremented "+sub);
		if(val==-1)
		player.Say((11),"Decremented "+sub);
	}
	
	else
	{
		if(val<1||val>10)
		{
			player.Say((11),"Incorrect stat value");
			return false;
		}
		
		if(stat=="st")
		target.ParamBase[(0)]=val;
		else if(stat=="pe")
