	Serializator serializer;
	void LoadEventSpawns()
	{
		serializer.Clear();
		if(!serializer.Load("event_spawns"))
		return;
		
		serializer.Get(eventItems).Get(eventCritters);
	}
	
	void SaveEventSpawns()
	{
		serializer.Clear();
		serializer.Set(eventItems).Set(eventCritters).Save("event_spawns");
	}
	
