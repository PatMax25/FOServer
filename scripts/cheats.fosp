	uint e_flag(array<uint>@LocData)
	{
		Item@flag=GetItem(LocData[0]);
		int team=LocData[1];
		int flagowner=LocData[2];
		int points=flag.Val4;
		Map@map=GetMap(LocData[4]);
		int dys=0;
		int members=0;
		string teamname="";
		int teamid=0;
		switch(team)
		{
			case 204:teamname="Enclave";teamid=0;break;
			case 206:teamname="Vault City";teamid=3;break;
			case 207:teamname="NCR";teamid=2;break;
			case 208:teamname="Defenders";teamid=4;break;
			case 209:teamname="Brotherhood of Steel";teamid=1;break;
			case 212:teamname="Master Army";teamid=5;break;
			case 231:teamname="Salvatories";teamid=6;break;
			case 232:teamname="Bishops";teamid=7;break;
			case 233:teamname="Wrights";teamid=8;break;
			case 234:teamname="Mordinos";teamid=9;break;
		}
		int ally=0;
		if(team==207)
		ally=209;
		if(team==209)
		ally=207;
		array<Critter@>players;
		int num=map.GetCritters(0,(0x10)|(0x03),players);
		for(int i=0;i<num;i++)
		{
			if(players[i].Param[(510)]>0)
			continue;
			dys=GetDistantion(flag.HexX,flag.HexY,players[i].HexX,players[i].HexY);
			if(dys<21&&players[i].Param[(89)]==team)
			{
				points+=10;
				members+=1;
				flag.Val4+=10;
				players[i].ParamBase[(1184)]+=3;
				flag.Update();
				players[i].Say((11),teamname+" is Capturing "+points/10+"%");
			}
			if(dys<21&&players[i].Param[(89)]!=team&&players[i].Param[(89)]!=ally)
			{
				if(points>0)
				{
					points-=10;
					flag.Val4-=10;
					players[i].ParamBase[(1184)]+=3;
					flag.Update();
					players[i].Say((11),teamname+" is Defending "+points/10+"%");
				}
			}
		}
		if(members==0&&points<1)
		{
			flag.Val0=0;
			flag.Val4=0;
			flag.Update();
			return 0;
		}
		if(points>999)
		{
			flag.ChangeProto(9000+team);
			flag.Val1=team;
			flag.Val0=0;
			flag.Val4=0;
			flag.Update();
			array<int>oilrig;
			AddBroadcastBuffer((8),((1500)*__TimeMultiplier/1000),0,teamname+" took control over Flag",oilrig,((1500)*__TimeMultiplier/1000),(0))
			.SendToAll();
			
			return 0;
		}
		return((2500)*__TimeMultiplier/1000);
	}
	
