	void ExecEvPonyAttack(array<string@>@command,Critter&player)
	{   
		
		if(command.length()<5||command.length()>6)
		{
			player.Say((11),"USAGE: `ev-ponyattack <hexX> <hexY> <targets> <howmanyattacks>.");
			player.Say((11),"<hexX> <hexY>: Coordinates to spawn.");
			player.Say((11),"Targets: players, npcs, all");
			player.Say((11),"How many attacks: Before go out. If 0 then infinite.");
			return;
		}
		
		uint16 hexX=0,hexY=0,howmanyattacks=0;
		
		if(!StrToInt(command[2],hexX)||!StrToInt(command[3],hexY))
		{
			player.Say((11),"Incorrect coordinates.");
			return;
		}
		
		uint8 findtypes;
		if(command[4]!="players"&&command[4]!="npcs"&&command[4]!="all")
		{
			player.Say((11),"Incorrect targets.");
			return;
		}
		else if(command[4]=="players")
		{
			findtypes=1;
		}
		else if(command[4]=="npcs")
		{
			findtypes=2;
		}
		else if(command[4]=="all")
		{
			findtypes=0;
		}
		
		if(!StrToInt(command[5],howmanyattacks))
		{
			player.Say((11),"Incorrect number of attacks.");
			return;
		}
		else
		{
			howmanyattacks=(((howmanyattacks)>(1000))?(1000):(((howmanyattacks)<(0))?(0):(howmanyattacks)));
		}
		
		if(howmanyattacks>0)
		{
			player.Say((11),"Now spawning attack of the ponies for "+howmanyattacks+" attacks. They will look for "+command[4]+"...");
		}
		else
		{
			player.Say((11),"Now spawning attack of the ponies for infinite attacks. They will look for "+command[5]+"...");
		}              
		
		Map@map=player.GetMap();
		if(!(@map!=null))
		{
			player.Say((11),"You're not on valid map.");
			return;
		}
		
		Item@Howitzer=map.AddItem(hexX,hexY,(25472),1);
		
		Critter@Commander=map.AddNpc(902,hexX-4,hexY+1,4,null,null,null);
		Critter@Soldier=map.AddNpc(902,hexX,hexY+1,5,null,null,null);
		
		array<uint>ponyattack={map.Id,Commander.Id,Soldier.Id,Howitzer.Id,findtypes,howmanyattacks,0,0,0};
		CreateTimeEvent(((__FullSecond)+(((5)*__TimeMultiplier))),"e_PonyPrepareAttack",ponyattack,false);   
		
	}
	
	uint e_PonyPrepareAttack(array<uint>@ponyattack)
	{
		Critter@Commander=GetCritter(ponyattack[1]);
		if(!(@Commander!=null))
		return 0;
		
		Critter@Soldier=GetCritter(ponyattack[2]);
		if(!(@Soldier!=null))
		return 0;
		
		Item@Howitzer=GetItem(ponyattack[3]);
		if(!(@Howitzer!=null))
		return 0;
		
		if(Commander.IsDead()&&Soldier.IsDead())
		return((Random(15,60))*__TimeMultiplier);
		
		if(Commander.IsDead())
		{
			Soldier.Say((2),"Waiting for commands...");
			return((Random(15,60))*__TimeMultiplier);
		}
		
		if(Soldier.IsDead())
		{
			Commander.Say((2),"Wake up pony-soldier! Don't pretend you are dead...");
			return((Random(15,60))*__TimeMultiplier);
		}
		
		Commander.Say((2),"Load a bullet for a howitzer!");
		
		Soldier.Animate((1),(28),Howitzer,true,true);
		
		CreateTimeEvent(((__FullSecond)+(((Random(2,5))*__TimeMultiplier))),"e_PonyFire",ponyattack,false);
		
		return 0;
	}
	
	uint e_PonyFire(array<uint>@ponyattack)
	{
		Map@map=GetMap(ponyattack[0]);
		if(!(@map!=null))
		return 0;
		
		Critter@Commander=GetCritter(ponyattack[1]);
		if(!(@Commander!=null))
		return 0;
		
		Critter@Soldier=GetCritter(ponyattack[2]);
		if(!(@Soldier!=null))
		return 0;
		
		array<Critter@>crits;
		switch(ponyattack[4])
		{
			case 0:map.GetCritters(0,(0x03),crits);break;
			case 1:map.GetCritters(0,(0x03)|(0x10),crits);break;
			case 2:map.GetCritters(0,(0x03)|(0x20),crits);break;
		}
		
		if((crits.length()==0&&ponyattack[4]==1)||
		(crits.length()==2&&(ponyattack[4]==0||ponyattack[4]==2)))
		{
			Commander.Say((2),"Damn... No targets...");
			Soldier.Say((2),":(");
			return((Random(15,60))*__TimeMultiplier);
		}
		else
		{
			uint16 randomindex;
			
			do
			{
				randomindex=Random(0,crits.length()-1);
			}while(crits[randomindex].GetProtoId()==902);
			
			if(!(@crits[randomindex]!=null))
			return 0;
			ponyattack[7]=crits[randomindex].HexX;
			ponyattack[8]=crits[randomindex].HexY;
		}
		
		Commander.Say((4),"FIRE!");
		
		Item@Howitzer=GetItem(ponyattack[3]);
		if(!(@Howitzer!=null))
		return 0;
		
		Soldier.Animate((1),(28),Howitzer,true,true);
		
		CreateTimeEvent(((__FullSecond)+(((1)*__TimeMultiplier))),"e_PonyExplode",ponyattack,false); 
		
		return 0;
	}
	
	uint e_PonyExplode(array<uint>@ponyattack)
	{
		
		Map@map=GetMap(ponyattack[0]);
		if(!(@map!=null))
		return 0;
		
		ExplodeEx(map,ponyattack[7],ponyattack[8],(4011),Random(3,4),Random(100,200),(7),3,ponyattack[1],0);
		
		ponyattack[6]++;
		
		if(ponyattack[5]==ponyattack[6])
		CreateTimeEvent(((__FullSecond)+(((5)*__TimeMultiplier))),"e_PonyDelete",ponyattack,false);
		else
		CreateTimeEvent(((__FullSecond)+(((5)*__TimeMultiplier))),"e_PonyPrepareAttack",ponyattack,false);
		
		return 0;
	}
	
	uint e_PonyDelete(array<uint>@ponyattack)
	{
		Critter@Commander=GetCritter(ponyattack[1]);
		if((@Commander!=null))
		DeleteNpc(Commander);
		
		Critter@Soldier=GetCritter(ponyattack[2]);
		if((@Soldier!=null))
		DeleteNpc(Soldier);
		
		Item@Howitzer=GetItem(ponyattack[3]);
		if((@Howitzer!=null))
		DeleteItem(Howitzer);
		
		return 0;
	}
	
