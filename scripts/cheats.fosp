	void ExecSetColorCommand(array<string@>@command,Critter&player)
	{ 
		
		int r=0,g=0,b=0,intens=30,radius=0,itemId=0;
		bool MakeLight=false;
		string[]sitem={"-i","-item"};
		
		if(StrToInt(GetParameterString(command,sitem),itemId))
		{
			Item@item=FindItem(player,itemId);
			if((@item!=null))
			{
				int arg=0;
				string[]red={"-r","-red"};
				string[]green={"-g","-green"};
				string[]blue={"-b","-blue"};
				
				string[]intensity={"-int","-intens"};
				string[]rad={"-rad","-radius"}; 
				
				cheatGetOption(command,MakeLight,"-light");
				
				if(StrToInt(GetParameterString(command,red),arg))
				if(arg<0||arg>255)player.Say((11),"Range of red color: 0 - 255");
				else r=arg;
				
				if(StrToInt(GetParameterString(command,green),arg))
				if(arg<0||arg>255)player.Say((11),"Range of green color: 0 - 255");
				else g=arg;
				
				if(StrToInt(GetParameterString(command,blue),arg))
				if(arg<0||arg>255)player.Say((11),"Range of red color: 0 - 255");
				else b=arg;
				
				item.LightColor=(uint((0xFF<<24)|(((r)&0xFF)<<16)|(((g)&0xFF)<<8)|((b)&0xFF)));
				player.Say((11),"New color red <"+r+">, green <"+g+"> and blue <"+b+"> is applied to item <"+item.Id+">.");  
				
				if(StrToInt(GetParameterString(command,intensity),arg))
				intens=arg;
				if(StrToInt(GetParameterString(command,rad),arg))
				radius=arg;
				
				item.LightIntensity=intens;
				item.LightDistance=radius; 
				
				if(!(((item.Flags)&((0x00008000)))!=0))
				((item.Flags)=(item.Flags)|((0x00008000))); 
				
				if(!((((item.LightFlags)&(1<<7))!=0))&&!MakeLight)
				(((item.LightFlags)=(item.LightFlags)|(1<<7)));
				
				if(((((item.LightFlags)&(1<<7))!=0))&&MakeLight)
				((item.LightFlags=((item.LightFlags)&(~(1<<7)))));
				
				item.Update();
			}
			else
			{
				player.Say((11),"Item not found. ");
				return;
			}
		}
		else
		{
			player.Say((11),"Missing -i argument.");
			return;
