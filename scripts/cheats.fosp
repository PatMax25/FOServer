					resultstring=resultstring+INTvalues[i];
					else
					resultstring=resultstring+", "+INTvalues[i];
				}
				resultstring+="...";
			}
			
			if(UINTvalues.length==0&&INTvalues.length==0)
			resultstring="No TimeEvent's values found...";
			
			player.Say((11),resultstring);
