	{
		endevent();
		finished=true;
		Map@map=cr.GetMap();
		Location@locwar=map.GetLocation();
		array<Map@>maps;
		locwar.GetMaps(maps);
		
		array<Critter@>players;  
		
		team1points=0;
		team2points=0;
		team3points=0;
		team4points=0;
		team5points=0;
		team6points=0;
		teamSALpoints=0;
		teamBISpoints=0;
		teamWRIpoints=0;
		teamMORpoints=0;
		bool alliance=false;
		if(teamwin>300)
		alliance=true;
		for(uint i=0;i<maps.length();i++)
		if((@maps[i]!=null))
		maps[i].GetCritters(0,(0x0F)|(0x10),players);
		
		for(uint16 i=0;i<players.length();i++)
		{
			array<Item@>items;
			uint n=players[i].GetItems(-1,items);
			for(uint j=0;j<n;j++)
			DeleteItem(items[j]);
			players[i].ToLife();
			UnsetMinigame(players[i]);
			players[i].ParamBase[(532)]=0;
			players[i].ParamBase[(523)]=0;
			if(!alliance)
			{
				if(players[i].Param[(89)]==teamwin)
				{
					players[i].ParamBase[(1184)]+=10000;
					AddScore(players[i],(51),players[i].ParamBase[(1184)]);
					players[i].ParamBase[(76)]+=exp;
					switch(reward)
					{
						case 1:{players[i].ParamBase[(76)]+=1000*players[i].Param[(77)];break;}
						case 2:{getImplant(players[i]);break;}
						case 3:{getT4part(players[i]);break;}
						case 4:{players[i].AddItem(currency(),exp);break;}
						case 5:{getStat(players[i]);break;}
						case 6:{getOPImplant(players[i]);break;}
						case 7:{getT4Realpart(players[i]);break;}
						case 8:{players[i].AddItem((25571),1);players[i].AddItem((25572),1);break;}
						case 9:{players[i].AddItem(tier4item(),1);break;}
