			BlockerDelete(player,((484)),optGroup,optAll);
			BlockerDelete(player,((820)),optGroup,optAll);
		}   
		
		else if(argSubcommand=="show"||argSubcommand=="hide")
		{
			cheatGetOption(command,optGroup,"-g");
			cheatGetOption(command,optAll,"-all");
			
