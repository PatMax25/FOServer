	if(level!=0)
	return;
	array<Critter@>followers;
	for(int i=0,j=GetFollowers(cr,true,followers);i<j;i++)
	(followers[i].FollowerVarBase[(941)]=((followers[i].FollowerVarBase[(941)])&(~((0x1)))));
}

