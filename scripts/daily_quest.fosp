void SpawnLoot(Map&map)
{
	uint16 x=1;
	uint16 y=2;
	for(int i=1;i<=10;i++)
	{
		map.GetEntireCoords(10,i,x,y);
		if(Random(1,5)==1)
		{
			Item@box1=map.AddItem(x,y,Random(130,131),1);
			addloot(box1);
			LockerClose(box1);
