	return(38);
	car.Deterioration=0;
	player.DeleteItem(Addpropermoney(tray),cost);
	BanksAddMoney(cost);
	FLog((7),GetCritterInfo(player)+" has repaired car "+PidToStr(car.GetProtoId())+" for "+cost+" on map "+player.GetMap().Id);
