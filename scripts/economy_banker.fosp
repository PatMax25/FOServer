	}
	
	if(!BankAccountRemoveMoney((player.ParamBase[(92)]),(player.ParamBase[(90)]),(10000)))
	{
		return(26);
	}
	
	uint result=BankSetAccountType((player.ParamBase[(92)]),(player.ParamBase[(90)]),(1));
	
	if(result!=(1))
	{
		return(27);
	}
	
	return(25);
}

