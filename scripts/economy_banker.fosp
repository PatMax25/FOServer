	uint result=BankWithdrawMoney((player.ParamBase[(92)]),player.Id,(player.ParamBase[(90)]),amount);
	
	if(result==(7))
	{
		player.Say((12),"I'm sorry, we can't let you withdraw that sum, we don't have that much money in our reserves.");
		return;
	}
	
	if(result==(5))
	{
