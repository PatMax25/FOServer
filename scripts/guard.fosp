	bool InitAccounts()
	{
		WLog("banks","Found "+GetNumberOfAccounts()+" accounts...");
		WLog("banks","Loading accounts...");
		for(uint i=0;i<GetNumberOfAccounts();i++)
		{
			BankAccount account;
