bool IsWeapon(Item@item)
{
	if(!(@item!=null))
	return false;
	
	uint pid=item.GetProtoId();
	if(pid==(600)||pid==(289))
	return false;
	
	return(item.GetType()==(3));
}

bool IsDrug(Item@item)
{
	if(!(@item!=null))
	return false;
	
	uint pid=item.GetProtoId();
	
	switch(pid)
	{
		case(87):
		case(53):
		case(259):
		case(110):
		return true;
		default:
	}
	return false;
} 

uint e_CheckWeapon(array<uint>@values)
{
	Critter@player=GetCritter(values[0]);
	Critter@guard=GetCritter(values[1]);
	
	if(!(@player!=null)||!((@guard!=null)))
	return 0;
	
	Item@item=player.GetItem(0,(1));
	if(IsWeapon(item)&&(player.Param[(72)]>0))
	AttackCritter(guard,player,0);
	
	return 0;
}

uint e_CheckDrug(array<uint>@values)
{
	Critter@player=GetCritter(values[0]);
	Critter@guard=GetCritter(values[1]);
	
	if(!(@player!=null)||!((@guard!=null)))
	return 0;
	
	Item@item=player.GetItem(0,(1));
	if(IsDrug(item)&&(player.Param[(72)]>0))
	AttackCritter(guard,player,0);
	return 0;
}

uint CreateWeaponCheckEvent(Critter&guard,Critter&player)
{
	int[]data={player.Id,guard.Id};
	return CreateTimeEvent(((__FullSecond)+(((5)*__TimeMultiplier))),"e_CheckWeapon",data,false);
}

uint CreateDrugCheckEvent(Critter&guard,Critter&player)
{
	int[]data={player.Id,guard.Id};
	return CreateTimeEvent(((__FullSecond)+(((5)*__TimeMultiplier))),"e_CheckDrug",data,false);
}

void CheckForDrugs(Critter&guard,Critter&crit)
{
	if(InDrugsAllowedZone(crit))
	return;
	
	Item@item=crit.GetItem(0,(1));
	
	if(IsDrug(item)&&(Random(0,1)==0))
	{
		guard.Say((4),"Put away your drugs immediately, it's not allowed here!");
		crit.Say((11),"Guard: |0xFFFF0000 PUT AWAY YOUR DRUGS IMMEDIATELY, IT'S NOT ALLOWED HERE!");
		CreateDrugCheckEvent(guard,crit);
	}
} 

void CheckForWeapons(Critter&guard,Critter&crit)
{
	
	if(((crit.CountItem((206))>0)||(crit.CountItem((209))>0))&&InNoBombZone(crit))
	{
		int rand=(crit.Param[(211)]-12*guard.Param[(1)]);
		rand=(((rand)>(95))?(95):(((rand)<(5))?(5):(rand)));
		if(Random(1,100)>rand)
		{
			AttackCritter(guard,crit);
			if(Random(0,2)==0)
			guard.Say((4),"Alert! Terrorist spotted!");
			crit.Say((11),"Guard: |0xFFFF0000 ALERT! TERRORIST SPOTTED!");
		}
	}
	
	Item@item=crit.GetItem(0,(1));
	
	if(InWeaponAllowedZone(crit))
	return; 
	
	if(IsWeapon(item))
	{
		guard.Say((4),"Put away your weapon immediately!!!");
		crit.Say((11),"Guard: |0xFFFF0000 PUT AWAY YOUR WEAPON IMMEDIATELY!!!");
		CreateWeaponCheckEvent(guard,crit);
	}
}

void CheckHealing(Critter&cr)
{
	if((@cr.GetMap()!=null)&&cr.GetMap().IsTurnBased())
	return; 
	
	if(cr.Param[(72)]<cr.Param[(7)])
	if(cr.Timeout[(230)]==0)
	{
		array<NpcPlane@>planes;
		cr.GetPlanes((33),planes);
		if(planes.length()>0)
		planes[0].IdentifierExt=cr.Id;
		else
		AddHealCritterPlane(cr,0,cr,false);
	} 
	
	if(cr.Damage[(502)]>0||cr.Damage[(503)]>0||
	cr.Damage[(504)]>0||cr.Damage[(505)]>0||cr.Damage[(506)]>0
	)
	if(cr.Timeout[(231)]==0)
	{
		array<NpcPlane@>planes;
		cr.GetPlanes((34),planes);
		if(planes.length()>0)
		planes[0].IdentifierExt=cr.Id;
		else
		AddDoctorCritterPlane(cr,0,cr,false);
	}
}

