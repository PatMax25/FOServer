void GuardOnIdle(Critter&guard)
{
	array<Critter@>crits;
	uint num=guard.GetCritters(false,(0x01)|(0x10),crits);
	
	for(uint i=0;i<num;i++)
	{
		if((GetCrittersDistantion(guard,crits[i])<6))
		{
			CheckForWeapons(guard,crits[i]);
			CheckForDrugs(guard,crits[i]);
		}
	}
	
	if(Random(0,9)==0)
	CheckHealing(guard);
}

bool DetectStealing(Critter&cr,Critter&target,Critter&thief)
{
	int dist=GetCrittersDistantion(cr,thief);
	if(dist>20)
	return false;
	int steal=thief.Param[(209)];
	steal-=(IsNight()?8:12)*cr.Param[(1)];
	steal+=dist*2;
	steal=(((steal)>((95+thief.Param[(6)]/3)))?((95+thief.Param[(6)]/3)):(((steal)<(dist))?(dist):(steal)));
	return(steal<=Random(1,100));
}

void GuardOnSomeoneSteal(Critter&guard,Critter&target,Critter&thief,bool success,Item&item,uint count)
{
	if(!success)
	{
		if(Random(0,15)==0&&(target.IsSee(guard)||thief.IsSee(guard)))
		{
			switch(Random(0,4))
			{
				case 0:
				guard.Say((2),"That was pathetic");
				break;
				case 1:
				guard.Say((2),"Now you just failed badly, "+thief.Name);
				break;
				case 2:
				guard.Say((2),"You didn't even try, did you?");
				break;
				case 3:
				guard.Say((2),thief.Name+", that was some kind of a joke");
				break;
				default:
				guard.Say((2),"Useless bastard");
			}
		}
		AttackCritter(guard,thief);
		return;
	}
	
	if(DetectStealing(guard,target,thief))
	{
		SetCritterIllegalFlag(thief,(0x00000001));
		if(target.IsPlayer()&&(target.IsSee(guard)||thief.IsSee(guard)))
		{
			if(Random(0,2)==0)
			guard.SayMsg((1),(0),(4503),"$name"+target.Name+"$thief"+thief.Name);
			
			target.SayMsg((11),(0),(4504),"$name"+thief.Name);
		}
	}
}

