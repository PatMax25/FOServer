		if((totalmoney+amount)>(2147483647))
		return(6);
		
		uint ret=bankaccounts[account].DepositMoney(amount,playerid);
		
		if(ret==(1))
		return this.AddMoney(amount)?(1):(8);
		else
		return ret;
	}
	
