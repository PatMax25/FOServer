void GuardOnMessage(Critter&guard,Critter&sender,int num,int val)
{
	if(num==(1000)&&!(IsFriend(guard,sender)))
	AttackCritter(guard,sender);
	
	else if(num==(1001))
	{
		if(val==(0))
		{
			if(guard.IsSeenBy(sender)&&Random(0,5)==0)
			guard.Say((4),sender.Name+", stop blocking the path for others!");
		}
		else if(val==(1))
		{
			if(guard.IsSeenBy(sender)&&Random(0,5)==0)
			guard.Say((2),sender.Name+", now we don't care.");
		}
	}
}

