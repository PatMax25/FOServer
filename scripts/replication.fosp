void SetStartLocation(Critter&cr)
{    
	
	uint x,y=0;      
	
	uint startloc=(357);
	switch(startloc)
	{
		case(357):
		x=1522;
		y=2028;
		break;
		case(358):
		x=1712;
		y=1438;
		break;
		case(359):
		x=1482;
		y=290;
		break;
		default:
		Log("Wrong location pid!");
		break;
	}
	
	uint locId=CreateLocation(startloc,x,y,null);
	Location@loc=GetLocation(locId);
	Map@map=loc.GetMapByIndex(0);
	if(!(@map!=null))
	return; 
	
	uint zoneX=cr.WorldX/__GlobalMapZoneLength;
	uint zoneY=cr.WorldY/__GlobalMapZoneLength;
	cr.SetFog(zoneX,zoneY,(0));
	cr.SetFog(zoneX-1,zoneY-1,(0));
	cr.SetFog(zoneX,zoneY-1,(0));
	cr.SetFog(zoneX+1,zoneY-1,(0));
	cr.SetFog(zoneX-1,zoneY,(0));
	cr.SetFog(zoneX+1,zoneY,(0));
	cr.SetFog(zoneX-1,zoneY+1,(0));
	cr.SetFog(zoneX,zoneY+1,(0));
	cr.SetFog(zoneX+1,zoneY+1,(0));
	
	cr.TransitToMap(map.Id,0);
	
	zoneX=cr.WorldX/__GlobalMapZoneLength;
	zoneY=cr.WorldY/__GlobalMapZoneLength;
	cr.SetFog(zoneX,zoneY,(3));
	cr.SetFog(zoneX-1,zoneY-1,(1));
	cr.SetFog(zoneX,zoneY-1,(1));
	cr.SetFog(zoneX+1,zoneY-1,(1));
	cr.SetFog(zoneX-1,zoneY,(1));
	cr.SetFog(zoneX+1,zoneY,(1));
	cr.SetFog(zoneX-1,zoneY+1,(1));
	cr.SetFog(zoneX,zoneY+1,(1));
	cr.SetFog(zoneX+1,zoneY+1,(1));
}

void SetReplicationTime(Critter&cr)
{
	Map@map=cr.GetMap();
	
	if((cr.IsNpc()&&!(((cr.Param[(534)]&((0x00000020)))!=0)))&&IsEncounterMap(map)&&cr.Param[(89)]!=(90))
	cr.ParamBase[(84)]=(-1); 
	
	int replTime=cr.Param[(84)];
	
	if(replTime==(-1))
	return;
	
	if(replTime<0)
	{ 
		
		if(cr.Param[(340)]>0)
		replTime=((45)*__TimeMultiplier);
		if(replTime==(-3))
		replTime=1;
		else
		replTime=10;
	}
	else if(replTime==0)
	{
		
		if(cr.IsPlayer())
		{
			if(cr.Param[(340)]>0)
			{
				replTime=((45-cr.Param[(4)])*__TimeMultiplier);
			}
			else if(cr.Param[(340)]==0)
			replTime=((60-cr.Param[(4)])*__TimeMultiplier);
			if(cr.Param[(404)]>0)
			{
				replTime*=5;
			}
		}
		else
		replTime=((30)*__TimeMultiplier*60);
	}
	else
	replTime=((replTime)*__TimeMultiplier);
	cr.TimeoutBase[(241)]=((__FullSecond)+(replTime));
}

void DropItems(Critter&cr)
{
	Map@dropMap=cr.GetMap();
	array<Item@>items; 
	
	cr.GetItems((5),items);
	if(items.length()>0)
	{
		DeleteItems(items);
		items.resize(0);
	} 
	
	cr.GetItems(-1,items);
	for(uint i=0,j=items.length();i<j;i++)
	{
		if(items[i].GetType()==(8))
		{
			DeleteItem(items[i]);
			@items[i]=null;
		}
		else if((((items[i].Flags)&((0x00000001)))!=0))
		@items[i]=null;
	}
	if((@dropMap!=null))
	MoveItems(items,dropMap,cr.HexX,cr.HexY);
	else
	DeleteItems(items);
}

Map@TryFindCompanionRespawn(Critter&cr,uint16&hx,uint16&hy,bool TryAny)
{
	Map@map=null;
	if(cr.Param[(947)]>0)
	{
		
		Map@basemap=GetMap(cr.Param[(947)]);
		return basemap;
	}
	if(!(@map!=null)&&(TryAny||(cr.FollowerVar[(936)]==(1))))
	{               
		
		Location@loc=GetNearestTentLocation(cr.FollowerVar[(933)],cr.WorldX,cr.WorldY);
		if((@loc!=null))
		{
			array<Map@>maps;
			uint count=loc.GetMaps(maps);
			for(uint m=0;m<count;m++)
			{
				if(GetEntireFreeHex(maps[m],(242),hx,hy))
				return(maps[m]);
			}
			
			FLog((10),"Companion "+cr.Id+": Found valid tent location for replication on pid "+loc.GetProtoId()+" but no free replication entires");
		}
		else
		{;};
	}      
	
	if(!(@map!=null)&&(TryAny||(cr.FollowerVar[(936)]==(3))))
	{
		{;};
		uint locId=cr.FollowerVar[(942)];
		Location@loc;
		if(locId!=0)
		@loc=GetLocation(locId);
		
		if(!(@loc!=null))
		{
			array<IFactionBase@>bases;
			if(GetFactionBases(cr.FollowerVar[(929)],bases)!=0)
			{
				@loc=GetLocation(bases[0].LocationId);
				cr.FollowerVarBase[(942)]=bases[0].LocationId;
			}
		}
		
		if((@loc!=null))
		{
			{;};
			@map=loc.GetMapByIndex(0);
			if(map.GetProtoId()==236||map.GetProtoId()==238)
			@map=loc.GetMapByIndex(1);
			if(GetEntireFreeHex(map,(242),hx,hy))
			return map;
			else
			{
				FLog((10),"Companion "+cr.Id+": Found valid base map for replication on pid "+map.GetProtoId()+" but no free replication entires");
				@map=null;
			}
		}
		
	}
	if(!(@map!=null)&&!TryAny)
	return TryFindCompanionRespawn(cr,hx,hy,true);
	
	if(!(@map!=null))
	FLog((10),"Companion "+cr.Id+": Searched trough all options, no valid map found or no free replication entires found on possible maps.");
	return map;
}

void ReplicateCritter(Critter&cr)
{
	if(cr.IsNpc()&&cr.Param[(84)]==(-1))
	return; 
	
	if(cr.IsNpc()&&
	(cr.Param[(84)]==(-2)||
	cr.Param[(84)]==(-3)))
	{
		
		if((not(cr.Param[(511)]>0)&&not(cr.Param[(523)]>0)&&not(cr.Param[(532)]>0))&&(((cr.Param[(534)]&((0x00000020)))!=0))&&
		cr.FollowerVar[(932)]==(2)||
		cr.FollowerVar[(932)]==(1)||
		cr.FollowerVar[(932)]==(4)||
		cr.FollowerVar[(932)]==(3))
		{
			Item@item=cr.GetItem(0,(3));
			if((@item!=null))
			cr.MoveItem(item.Id,item.GetCount(),(0));
			DropItems(cr);
		}
		
		DeleteNpc(cr);
		return;
	}     
	
	if((cr.IsPlayer()&&cr.GetAccess()==(0))&&GetLvar(cr,(1207))>0)
	{
		if(cr.ToLife())
		{
			cr.ParamBase[(72)]=GetLvar(cr,(1207));
			SetLvar(cr,(1207),0);
			cr.ParamBase[(532)]=0;
			cr.ParamBase[(511)]=0;
		}
		else
		{
			cr.TimeoutBase[(241)]=((__FullSecond)+(((1)*__TimeMultiplier*60)));
		}
		return;
	}
	if(IsFlaggedAsIllegal(cr)&&cr.GetMapId()!=0)
	UnsetCritterIllegalFlag(cr);
	
	ClearInfluenceBuffer(cr);
	GameVar@casinovar=GetLocalVar((11201),cr.Id);
	if((@casinovar!=null))
	casinovar=0;  
	
	Map@map=null;
	uint16 hx=0,hy=0;
	bool room=false;
	if(cr.IsPlayer())
	{
		Map@deathMap=cr.GetMap();
		
		if((@deathMap!=null)&&(IsJail(deathMap)||IsWarzone(deathMap)))
		{
			@map=deathMap;
			array<Entire>entires;
			ParseEntires(map,entires,0);
			if(entires.length()>0)
			{
				int i=Random(0,entires.length()-1);
				hx=entires[i].HexX;
				hy=entires[i].HexY;
			}
		}
		else if((@deathMap!=null)&&deathMap.GetLocation().GetProtoId()==(50)&&IsInsideArena(cr))
		{
			@map=deathMap;
			array<Entire>entires;
			ParseEntires(map,entires,242);
			if(entires.length()>0)
			{
				int i=Random(0,entires.length()-1);
				hx=entires[i].HexX;
				hy=entires[i].HexY;
			}
		}
		else
		{
			
			if((@deathMap!=null))
			{
				if(deathMap.GetLocation().GetProtoId()==(50)&&IsDeadInsideLobby(cr))
				DropDrugsEffectsHinkley(cr);
				
				Item@dynRespawn=GetDSpawn(deathMap,cr.Param[(147)]);
				if((@dynRespawn!=null))
				{
					@map=GetMap(dynRespawn.MapId);
					hx=dynRespawn.HexX;
					hy=dynRespawn.HexY;
				}
			}
			
			if(!(@map!=null))
			{   
				
				@map=GetNearestReplicatorMap(cr);
				GetEntireFreeHex(map,(242),hx,hy);  
				
			}
		}
		
		if(!(@map!=null)||(hx==0&&hy==0))
		{
			
			cr.TimeoutBase[(241)]=((__FullSecond)+(((1)*__TimeMultiplier*60)));
			return;
		}
		
		if((not(cr.Param[(511)]>0)&&not(cr.Param[(523)]>0)&&not(cr.Param[(532)]>0)))
		{
			if(!(map.GetLocation().GetProtoId()==(50)&&IsInsideArena(cr)))
			DropItems(cr);
		}
	}
	else
	{
		cr.DropPlanes();
		cr.ClearEnemyStackNpc();
		bool Tried=false;
		if(cr.FollowerVar[(932)]==(1))
		{
			
			array<Item@>items;
			cr.GetItems(-1,items);
			
			for(uint i=0,j=items.length();i<j;i++)
			{
				if(items[i].GetProtoId()==(209)||items[i].GetProtoId()==(206))
				{
					DeleteItem(items[i]);
				}
			}
			
			@map=TryFindCompanionRespawn(cr,hx,hy,false);
			if(!(@map!=null))
			{
				{;};
				DeleteNpc(cr);
			}
			array<Entire>entires;
			ParseEntires(map,entires,242);
			if(entires.length()>0)
			{
				int i=Random(0,entires.length()-1);
				hx=entires[i].HexX;
				hy=entires[i].HexY;
			}
			if((@cr!=null))
			cr.FollowerVarBase[(937)]=(((cr.FollowerVar[(937)]-(0))>(100))?(100):(((cr.FollowerVar[(937)]-(0))<(0))?(0):(cr.FollowerVar[(937)]-(0))));
		}
		else
		{
			cr.ParamBase[(110)]=0;
			@map=cr.GetMap();
			if(!(@map!=null))
			{
				DeleteNpc(cr);
				return;
			}
			hx=cr.HexX;
			hy=cr.HexY;
			
			if(cr.Param[(113)]!=0)
			{
				Item@block=::GetItem(cr.Param[(113)]);
				if((@block!=null))
				DeleteItem(block);
				cr.ParamBase[(113)]=0;
			}
			
			if(!map.IsHexPassed(hx,hy))
			{
				bool founded=false;
				for(int x=-1;x<=1;x++)
				{
					for(int y=-1;y<=1;y++)
					{
						if(x==0&&y==0)
						continue;
						if(__MapHexagonal)
						{
							if((hx%2)==1&&((x==-1&&y==1)||(x==1&&y==1)))
							continue;
							if((hx%2)==0&&((x==-1&&y==-1)||(x==1&&y==-1)))
							continue;
						}
						
						if(map.IsHexPassed(hx+x,hy+y))
						{
							hx+=x;
							hy+=y;
							founded=true;
							break;
						}
					}
					if(founded)
					break;
				}
				
				if(!founded)
				{
					
					cr.TimeoutBase[(241)]=((__FullSecond)+(((1)*__TimeMultiplier*60)));
					return;
				}
			}
		}
	}
	
	if(!(@map!=null))
	{
		Log("Cannot find replication map, critter "+cr.Id);
		return;
	} 
	
	if(map.GetLocation().GetProtoId()==(50)&&IsInsideArena(cr))
	{
		cr.TransitToMap(map.Id,hx,hy,Random(0,5));
		OnTeleportToLobby(cr);
	}
	else
	{
		cr.TransitToMap(map.Id,hx,hy,Random(0,5));
		cr.DamageBase[(502)]=0;
		cr.DamageBase[(503)]=0;
		cr.DamageBase[(504)]=0;
		cr.DamageBase[(505)]=0;
		cr.DamageBase[(506)]=0;
		
		cr.ParamBase[(72)]=10;
		if(cr.IsNpc())
		cr.ParamBase[(72)]=cr.Param[(7)];
		
		if(IsWarzone(map))
		cr.ParamBase[(72)]=cr.Param[(7)]; 
		
	}  
	
	cr.ToLife();  
	
	SetLvar(cr,(1235),0);
	cr.ParamBase[(75)]=cr.Param[(8)]*100;
	
	if((cr.IsPlayer()&&cr.GetAccess()==(0)))
	cr.ParamBase[(532)]=0;
	
	DropPoison(cr);
	CapRadiation(cr); 
	
	cr.DropTimers();       
	
}
