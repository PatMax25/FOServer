	for(uint i=0,j=Hospitals.length();i<j;i++)
	{
		@loc=GetLocationByPid(Hospitals[i],0);
		if(!(@loc!=null))
		{
			Log("Invalid hospital location, looking further...");
			continue;
		}
		tmp=(sqrt(((loc.WorldX)-(player.WorldX))*((loc.WorldX)-(player.WorldX))+((loc.WorldY)-(player.WorldY))*((loc.WorldY)-(player.WorldY))));  
		
		{;};
		if(tmp<dist)
		{
			dist=tmp;
			@nearest=@loc;
		}
	}
	if(!(@nearest!=null))
	{
		Log("Couldn't obtain nearest hospital");
		return null;
	}
	
	return nearest.GetMapByIndex(0);
}
