			}
			ChatSayEditBoxOpt.Text("");
			ChatSayEditBoxOpt.TextOptions((5),((uint((0xFF<<24)|(((0)&0xFF)<<16)|(((0xFF)&0xFF)<<8)|((0)&0xFF)))),(0x0200));
			return true;
		}
		if(key!=0x01)return true;
		else return false;
	}
}

