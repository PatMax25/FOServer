void GetPerk(Item&item,uint bonus,uint level)
{
	bool is_a_melee=false;
	if(item.Proto.Weapon_Skill_0==((204))||item.Proto.Weapon_Skill_0==(203))
	is_a_melee=true;
	if(level==1&&item.Val6!=0)
	return;
	if(level==2&&item.Val7!=0)
	return;
	if(level==3&&item.Val8!=0)
	return;
	if(level==4&&item.Val5!=0)
	return;
	if(level==5&&item.Val4!=0)
	return;
	Critter@cr=GetCritter(item.CritId);
	if(level==7&&item.Val2>99&&item.Val2<110)
	return;
	switch(level)
	{
		case 1:item.Val6=bonus;break;
		case 2:item.Val7=bonus;break;
		case 3:item.Val8=bonus;break;
		case 4:
		{
			if((@cr!=null)&&cr.IsPlayer())
			{
				cr.ParamBase[(801)]=item.Id;
				RunDialog(cr,9302,cr.HexX,cr.HexY,true);
			}
			else
			{
				if(item.GetType()==(1))
				item.Val5=Armor_perks[Random(0,26)];
				if(item.GetType()==(3))
				item.Val5=is_a_melee?Weapon_perks_melee[Random(0,15)]:Weapon_perks[Random(0,15)];
			}
			break;
		}
		case 5:item.Val4=bonus;break;
		case 6:item.Val3=bonus;break;
		case 7:item.Val2=bonus;break;
	}
	item.Val1=level;
	
	item.Update();
}
