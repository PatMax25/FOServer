bool UseSkillOn(Critter&npc,uint crId,NpcPlane&plane)
{
	Critter@cr=::GetCritter(crId);
	if(!(@cr!=null))
	return true;
	
	if(npc.GetMapId()!=cr.GetMapId())
	return true;
	
	bool firstAid=plane.Identifier==(33);
	bool doctor=plane.Identifier==(34);
	if(firstAid||doctor)
	if(npc.Timeout[firstAid?(230):(231)]>0)
	return true;
	
	if(GetCrittersDistantion(npc,cr)<2)
	{
		if(npc.Param[(75)]<int(__RtApCostUseSkill))
		{
			npc.Wait(1000);
			if(firstAid)
			AddHealCritterPlane(npc,plane.Priority,cr,true);
			else if(doctor)
			AddDoctorCritterPlane(npc,plane.Priority,cr,true);
			else
			return false;
			return true;
		}
		if(npc.Id!=cr.Id)
		npc.SetDir(GetDirection(npc.HexX,npc.HexY,cr.HexX,cr.HexY));
		npc.Animate(0,(28),null,true,true);
		npc.ParamBase[(75)]-=100*__RtApCostUseSkill;
		if(firstAid)
		critter_use_skill(npc,(206),cr,null,null);
		else if(doctor)
		critter_use_skill(npc,(207),cr,null,null);
		else
		critter_use_skill(npc,(-6),cr,null,null);
		
		if(firstAid||doctor)
		npc.Action((10),1,null);
		return true;
	}
	else
	{
		if(firstAid)
		AddHealCritterPlane(npc,plane.Priority,cr,true);
		else
		AddDoctorCritterPlane(npc,plane.Priority,cr,true);
		
		return true;
	}
}     

