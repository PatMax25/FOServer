int _PlaneEnd(Critter&cr,NpcPlane&plane,int reason,Critter@someCr,Item@someItem)
{
	if(plane.Identifier==(29))
	{
		if(reason==(30))
		{
			cr.TransitToGlobal(false);
			return(2);
		}
		
		else if(reason==(33)||reason==(32))
		{
			uint16 hx=plane.Walk_HexX;
			uint16 hy=plane.Walk_HexY;
			Map@map=cr.GetMap();
			if((@map!=null)&&map.GetEntireCoords(0,0,hx,hy)&&FindPreferredGridApprox(map,hx,hy))
			{
				plane.Walk_HexX=hx;
				plane.Walk_HexY=hy;
			}
			else
			return(2);
		}
	}
	return(0);
}

