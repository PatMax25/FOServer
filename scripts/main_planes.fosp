		Critter@cr=GetCritter(Companions[i]);
		if(!(@cr!=null))
		{  
			
			Companions.removeAt(i);
		}        
		
	}
	
	for(uint i=0;i<Companions.length();i++)
	{
		Critter@cr=GetCritter(Companions[i]);
		if(!(@cr!=null))
		continue;
		
		if(Random(0,2)==0)
