bool npc_plane_begin(Critter&npc,NpcPlane&plane,int reason,Critter@someCr,Item@someItem)
{  
	
	if(plane.Type==(1))
	{
		Critter@target=GetCritter(plane.Attack_TargId);
		if(!(@target!=null))
		return false;
		
		if(((npc.Param[(534)]&((0x00000800)))!=0))
		{
