		int ramForce=Random(-2,2)+npc.ParamBase[(10)]+npc.ParamBase[(0)]-target.ParamBase[(0)];
		if(ramForce<-2)
		npc.ToKnockout(((Random(0,1)==0)?(82):(83)),((Random(0,1)==0)?(86):(87)),((Random(0,1)==0)?(88):(89)),4,npc.HexX,npc.HexY);
		if(ramForce>-4)
		target.ToKnockout(((Random(0,1)==0)?(82):(83)),((Random(0,1)==0)?(86):(87)),((Random(0,1)==0)?(88):(89)),8,target.HexX,target.HexY);
		if(ramForce>0)
