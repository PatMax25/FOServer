void FireInTheHole(Critter&caller,Item&armedBomb,uint16 dangerDist)
{
	
	uint eventTime=0;
	array<uint>eventValues;
	if(GetTimeEvent(uint(armedBomb.Val1),eventTime,eventValues))
	{
		
		uint bombMapID=0;
		uint16 bombX=0,bombY=0;
		if(!GetItemCoordinates(armedBomb,bombMapID,bombX,bombY))
		return; 
		
		if(dangerDist==0)
		dangerDist=5;
		uint waitTime=eventTime*60; 
		
		Map@map=GetMap(bombMapID);
		array<Critter@>crits;
		uint critsNum=map.GetCrittersHex(bombX,bombY,dangerDist,(0x01)|(0x20),crits);
		
		uint16 safeDist=dangerDist+3;
		
		for(uint i=0;i<critsNum;i+=1)
		{
			if(HasIntelligentBrain(crits[i]))
			{
				uint16 x=crits[i].HexX;
				uint16 y=crits[i].HexY;
				uint dir=GetDirection(bombX,bombY,x,y);
				uint dist=GetDistantion(bombX,bombY,x,y);
				
				map.MoveHexByDir(x,y,dir,safeDist-dist);
				map.MoveHexByDir(x,y,Random(0,5),2);
				if(FindFirstFreeHex(map,x,y,dir,6))
				{
					AddWalkPlane(crits[i],(50)+20,(39),waitTime,x,y,(((((dir))+(3))%6)),true,0);
					if(IsEncounterMap(map))
					crits[i].SetHomePos(x+Random(-2,2),y+Random(-2,2),Random(0,5));
					if(@crits[i]!=@caller)
					switch(Random(0,13))
					{
						case 0:
						crits[i].Say((2),"Fuck.");
						break;
						case 1:
						crits[i].Say((2),"Oh shit...");
						break;
						case 2:
						crits[i].Say((2),"Mommy!");
						break;
						case 3:
						crits[i].Say((2),"Help!!!");
						break;
						case 4:
						crits[i].Say((2),"Not again.");
						break;
						case 5:
						crits[i].Say((2),"Hide!");
						break;
						case 6:
						crits[i].Say((2),"Aaaaah!");
						break;
					}
				}
			}
		} 
		
		SetTimeEvent(armedBomb.Val1,eventTime,eventValues);
	}
}   

bool LootCritter(Critter&npc,uint crId,bool lootEx,uint16 hexX,uint16 hexY)
{
	Map@map=npc.GetMap();
	Critter@cr=::GetCritter(crId);
	array<Item@>items;
	uint num=0;
	if((@cr!=null)&&cr.IsDead())
	{
		uint start=GetTick();
		
		num+=cr.GetItems((0),items);
		num+=cr.GetItems((1),items);
		num+=cr.GetItems((2),items);
		num+=map.GetItems(cr.HexX,cr.HexY,items);
		
		for(uint i=0;i<num;i++)
		{
			uint8 type=items[i].GetType();
			if(type==(1)||type==(2)||type==(3)||type==(4)||type==(5)||type==(7))
			{
				if((((items[i].Flags)&((0x08000000)))!=0))
				{
					uint16 pid=items[i].GetProtoId();
					if(pid==(206))
					{
						npc.Say((3),"There is a bomb here! Run!!!");
						FireInTheHole(npc,items[i],0);
						return true;
					}
					else
					{
						if(((npc.Param[(534)]&((0x00000008)))!=0)&&(items[i].Proto.Weight>0))
						DeleteItem(items[i]);
						else
						{
							MoveItem(items[i],0,npc);
							npc.EventMoveItem(items[i],(255));
						}
					}
				}
			}
		}
		items.resize(0);          
		
		npc.Animate(0,(27),null,true,true);
		if(!lootEx)
		{
			
			array<Critter@>crits;
			num=npc.GetCritters(false,(0x04),crits);
			for(uint i=0;i<num;i++)
			{
				if((crits[i].Param[(532)]>0))
				continue;
				if(crits[i].IsPlayer()||(crits[i].IsNpc()&&IsHumanoid(crits[i])))
				{
					uint n=map.GetItems(crits[i].HexX,crits[i].HexY,items);
					if(crits[i].ItemsCount()>0||n>0)
					{
						
						uint c=0;
						for(uint i=0;i<n;i++)
						{
							uint8 type=items[i].GetType();
							if(type==(1)||type==(2)||type==(3)||type==(4)||type==(5)||type==(7))
							{
								c++;
								break;
							}
						}
						if(c>0)
						{
							AddWalkPlane(npc,0,(31),crits[i].Id,crits[i].HexX,crits[i].HexY,6,true,1);
							break;
						}
					}
				}
			}
		}
		uint t=GetTick()-start;
		{if(cr.IsPlayer()&&cr.GetAccess()==(3)) cr.Say((11),"Handled cr looting in: "+t+"ms.");};
	}
	else
	{
		npc.Animate(0,(27),null,true,true);
		num+=map.GetItems(hexX,hexY,items);
		if(IsTown(map))
		DeleteItems(items);
		else
		for(uint i=0;i<num;i++)
		{
			uint8 type=items[i].GetType();
			if(type==(1)||type==(2)||type==(3)||type==(4)||type==(5)||type==(7))
			{
				if((((items[i].Flags)&((0x08000000)))!=0))
				{
					uint16 pid=items[i].GetProtoId();
					if(pid==(206))
					{
						npc.Say((3),"There is a bomb here! Run!!!");
						FireInTheHole(npc,items[i],0);
						return true;
					}
					else
					{
						if(((npc.Param[(534)]&((0x00000008)))!=0)&&(items[i].Proto.Weight>0))
						DeleteItem(items[i]);
						else
						{
							MoveItem(items[i],0,npc);
							npc.EventMoveItem(items[i],(255));
						}
					}
				}
			}
		}
	}
	npc.Wait(__Breaktime);
	return true;
}

bool PlaneEndSuccess(Critter&npc,NpcPlane&plane)
{
	if(plane.Identifier==(30)||plane.Identifier==(31))
	{
		return LootCritter(npc,plane.IdentifierExt,(plane.Identifier==(31)),plane.Walk_HexX,plane.Walk_HexY);
	}
	else if(plane.Identifier==(33)||plane.Identifier==(34)||plane.Identifier==(36))
	{
		return UseSkillOn(npc,plane.IdentifierExt,plane);
	}
	else if(plane.Identifier==(39))
	{
		npc.Wait(plane.IdentifierExt);
	}
	else if(plane.Identifier==(41))
	{
		RamIntoCritter(npc,plane.IdentifierExt,true);
	}
	else if(plane.Identifier==(40))
	{
		uint ownerId=plane.IdentifierExt;
		Critter@wpnOwner=GetCritter(ownerId);
		if((@wpnOwner!=null)&&wpnOwner.IsLife())
		{
			if(wpnOwner.HexX==plane.Walk_HexX&&wpnOwner.HexY==plane.Walk_HexY)
			{
				npc.Say((2),"I think this is yours.");
				array<Item@>items;
				uint cnt=npc.GetItems(-1,items);
				for(uint i=0;i<cnt;i++)
				{
					if(wpnOwner.Id==uint(GetLvar(items[i].Id,(9010))))
					MoveItem(items[i],0,wpnOwner);
				}
				if(wpnOwner.IsNpc())
				ArmBestWeapon(wpnOwner);
			}
			else
			{
				AddWalkPlane(npc,(20),(40),ownerId,wpnOwner.HexX,wpnOwner.HexY,0xFF,false,1);
			}
		}
	}
	return true;
}

