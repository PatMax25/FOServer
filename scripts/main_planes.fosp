			return false;
		}    
		
		if((((target.Param[(534)]&((0x00002000)))!=0))||((npc.Param[(534)]&((0x00002000)))!=0))
		return false;
		
		if((((target.Param[(534)]&((0x00001000)))!=0)&&!(((npc.Param[(534)]&((0x00000020)))!=0))))
		return false;
		if((((npc.Param[(534)]&((0x00001000)))!=0)&&target.IsNpc()&&!(((target.Param[(534)]&((0x00000020)))!=0))))
		return false;
		
		if((npc.FollowerVar[(933)]>0)&&((npc.FollowerVar[(933)]==target.FollowerVar[(933)])||(uint(npc.FollowerVar[(933)])==target.Id)))
		return false;                 
		
		if((npc.IsNpc()&&!(((npc.Param[(534)]&((0x00000020)))!=0)))&&(target.IsNpc()&&!(((target.Param[(534)]&((0x00000020)))!=0))))
