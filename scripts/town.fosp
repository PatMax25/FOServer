}

bool d_CanBecomeMilitia(Critter&player,Critter@npc)
{
	ITown@town=RetrieveTown(player);
	if(!(@town!=null))
	return false;
	
	return(town.IsMilitiaEnabled()&&IsControllingTown(player)&&(CountMilitia(player.GetMap())<int(town.GetMilitiaMax())));
}

