void AddReputation(Critter@cr,uint index,int val,int cap)
{
	if(index<2||index>99){{;};return;};
	int rep=cr.Reputation[index];
	if(rep>=cap)
	return;
	rep+=val;
	if(rep>cap)
	rep=cap;
	cr.ReputationBase[index]=rep;
}        

void SubReputation(Critter@cr,uint index,int val,int cap)
{
	if(index<2||index>99){{;};return;};
	int rep=cr.Reputation[index];
	if(rep<=cap)
	return;
	rep-=val;
	if(rep<cap)
	rep=cap;
	cr.ReputationBase[index]=rep;
}       

