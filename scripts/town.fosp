bool d_CanRecruitMilitia(Critter&player,Critter@npc)
{
	
	ITown@town=RetrieveTown(player);
	if(!(@town!=null))
	return false;
	if(!IsControllingTown(player))
	return false;
	
	if(town.GetVersion()==(2))
	return(town.GetControllingInfluence()>=(25));
	else
	return(town.IsMilitiaEnabled());
}

