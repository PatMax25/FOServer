class CTown:ITown
{
	uint tcVersion;
	array<float>factionInfluence;
	
	bool militiaEnabled;
	uint militiamax;
	uint status;
	uint faction;
	uint previousfaction;
	uint memberNearbyReq;
	uint memberInfluenceReq;
	uint memberTotalReq;
	uint npcDeaths;
	uint controlCycles;
	uint rewardCapsStart;
	uint rewardCaps;
	uint mapid;
