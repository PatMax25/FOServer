			uint contitems=ContItems.length()/4;
			for(uint k=0;k<contitems;k++)
			if(uint(Random(1,100))<=ContItems[4*k+1])
			items[i].AddItem(ContItems[4*k],Random(ContItems[4*k+2],ContItems[4*k+3]),0);
			break;
		}
