		}
		
		(npc.ParamBase[(534)]=npc.Param[(534)]|((0x00000001)));
		
		if(NoItems)
		return true; 
		
		uint items=Items.length()/4;
		for(uint i=0;i<items;i++)
		if(uint(Random(1,100))<=Items[4*i+1])
		npc.AddItem(Items[4*i],Random(Items[4*i+2],Items[4*i+3])); 
		
