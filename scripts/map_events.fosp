void RemoveItems(Critter&mob)
{
	array<Item@>items;
	mob.GetItems(-1,items);
	for(uint8 i=0;i<items.length();i++)
	{
		if((@items[i]!=null))
		{
			mob.MoveItem(items[i].Id,1,(0));
			DeleteItem(items[i]);
		}
		
	}
	mob.Wait(2000);
}

