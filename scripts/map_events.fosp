	{
		mob.ParamBase[(64)]=44;
		mob.ParamBase[(398)]=1;
		mob.ParamBase[(530)]=1;
		mob.ParamBase[(10)]=15;
	}
	
	mob.SetEvent((0),"_MobIdle");
	
