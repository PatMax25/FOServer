	{
		
		mob.SetEvent((2),"_DeleteMob");  
		
		array<uint>keeppushing={map.Id,170};
		CreateTimeEvent(((__FullSecond)+(((1)*__TimeMultiplier*60))),"e_PushForward",keeppushing,true); 
		
	}
