void _DeleteMob(Critter&mob,Critter@killer)
{
	
	uint playerId=0;
	if(!(@mob!=null))
	return;
	if(mob.GetMap().GetNpcCount((203),(0x03)|(0x20))<1)
	{
		ClearEnemyStacksOnMap(mob.GetMap());
	}
	if((@killer!=null)&&killer.IsPlayer()&&mob.GetMap().GetLocation().IsGuarded()&&IsNotDungInGuardedLoc(mob.GetMap()))
	{
		AddScore(killer,(14),1);
		
		playerId=killer.Id;
	}
	
	if((@killer!=null)&&(((killer.Param[(534)]&((0x00000020)))!=0))&&killer.ParamBase[(933)]!=0&&mob.GetMap().GetLocation().IsGuarded()&&IsNotDungInGuardedLoc(mob.GetMap()))
	{
		Critter@player=GetCritter(killer.FollowerVar[(933)]);
		if(!(@player!=null))
		return;
		
		playerId=player.Id;
		
		AddScore(player,(14),1);
		
	}
	
	if((@killer!=null)&&(killer.IsPlayer()||((((killer.Param[(534)]&((0x00000020)))!=0))&&killer.ParamBase[(933)]!=0))&&mob.GetMap().GetLocation().IsGuarded()&&IsNotDungInGuardedLoc(mob.GetMap()))
	{ 
		
		array<Item@>rewards;
		
		mob.GetItemsByType((3),rewards);
		mob.GetItemsByType((5),rewards); 
		
		if(rewards.length()>0)
		{
			Item@reward=rewards[Random(0,rewards.length()-1)];
			if(!(@reward!=null))
			return;
			
			if(reward.GetProtoId()!=(270)&&reward.GetProtoId()!=(393)
			&&reward.GetProtoId()!=(371)&&reward.GetProtoId()!=(372)
			&&reward.GetProtoId()!=(427)&&reward.GetProtoId()!=(489)
			&&reward.GetProtoId()!=(498)&&reward.GetProtoId()!=(290)
			&&reward.GetProtoId()!=(291)&&reward.GetProtoId()!=(518)
			&&reward.GetProtoId()!=(520)&&reward.GetProtoId()!=(530)
			&&reward.GetProtoId()!=(531)&&reward.GetProtoId()!=(421)
			&&reward.GetProtoId()!=(753))
			{
				
				Critter@player=GetCritter(playerId);
				if(!(@player!=null))
				return;
				
				SetLexem(reward,"$rewardfor",player.Name);  
				
				Map@map=mob.GetMap();
				if(!(@map!=null))
				return;
				
				Critter@rewardgiver=map.GetNpc((210),(0x0F)|(0x20),0);
				if(!(@rewardgiver!=null))
				return;
				rewardgiver.ParamBase[(532)]=1;
				rewardgiver.ParamBase[(512)]=1;
				MoveItem(reward,1,rewardgiver);
				
			}
		}
	}  
	
	CreateTimeEvent(((__FullSecond)+(((25)*__TimeMultiplier))),"e_deleteMob",mob.Id,true);
}

