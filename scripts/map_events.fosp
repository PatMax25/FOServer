bool _AlertE(Critter&mob,Critter&attacker)
{
	if(!(@mob!=null))
	return true;
	
	if(!(@attacker!=null))
	return true;
	
	if(attacker.Param[(89)]==(204))
	return true;
	
	uint radius=(mob.Param[(0)]+mob.Param[(2)])*2;
	
	array<Critter@>mobstoalert;
	mob.GetMap().GetCrittersHex(mob.HexX,mob.HexY,radius,(0x03)|(0x20),mobstoalert);
	
	for(uint8 i=0;i<mobstoalert.length();i++)
	{
		if(!(@mobstoalert[i]!=null))continue;
		if(mobstoalert[i].Param[(89)]==(204))
		{
			mobstoalert[i].AddEnemyInStack(attacker.Id);
			AddAttackPlane(mobstoalert[i],0,attacker);
		}
		
	}
	
	return false;
}

void _MobIdleE(Critter&mob)
{
	if(!(@mob!=null))
	return;
	
	if(!mob.IsLife())
	{
		mob.Wait((1000));
		return;
	}  
	
	array<Critter@>crits;
	
	uint num=mob.GetCritters(false,(0x03),crits);
	
	uint dist=GetDistance(mob);
	for(uint i=0;i<num;i++)
	{
		
		if(TryAttackE(mob,crits[i]))return;
		
	}
	
	if(crits.length()>0)
	{
		MoveMob(mob,false);
		mob.Wait((200));
	}
	else
	
	{
		MoveMob(mob,false);
		mob.Wait((200));
	}
	
}

bool TryAttackE(Critter&mob,Critter@targetCr)
{
	
	if((targetCr.Param[(89)]!=(204)))
	{
		NpcPlane@plane=mob.GetCurPlane();
		if(((@plane!=null))&&(plane.Identifier!=(1)))
		mob.DropPlanes();
		
		mob.AddEnemyInStack(targetCr.Id);
		AddAttackPlane(mob,0,targetCr);
		return true;
	}
	return false;
}

