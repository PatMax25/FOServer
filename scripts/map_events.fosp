	mob.ParamBase[(523)]=1;
	mob.ParamBase[(532)]=1;
	mob.ParamBase[(522)]=1;
	
	mob.ParamBase[(512)]=1;
	mob.ParamBase[(524)]=0;  
	
	(mob.ParamBase[(534)]=mob.Param[(534)]|((0x00000004)));
	if(firstTime&&!MobForEvent)
	{
