void _MobIdle(Critter&mob)
{
	if(!(@mob!=null))
	return;
	
	if(!mob.IsLife())
	{
		mob.Wait((1000));
		return;
	}
	
	bool MobForEvent=(mob.Param[(187)]==(2)?true:false);
	
	mob.SetEvent((4),null);
	
	if(Random(0,200)==0&&!IsSierraRobot(mob))
	{ 
		
		if(!MobForEvent)MoveMobToFreeHex(mob);
	}         
	
	array<Critter@>crits;
	
	uint num=mob.GetCritters(false,(0x03),crits);    
	
	uint dist=GetDistance(mob);
	for(uint i=0;i<num;i++)
	{    
		
		if(TryAttack(mob,crits[i]))return;                 
		
	}
	
	if(crits.length()>0)
	{
		MoveMob(mob,false);
		mob.Wait((200));
		
	}
	else  
	
	{
		MoveMob(mob,false);
		
		mob.Wait((200));
	}
	
}

