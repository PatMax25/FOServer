bool d_canenclave(Critter&player,Critter@npc)
{
	if(!allowenclave)
	return false;
	if(player.Param[(89)]>20)
	return false;
	
	int teamA=0;
	int teamB=0;
	uint teamACount=0;
	uint teamBCount=0;
	bool myteam=false;
	if(npc.Param[(124)]>0)
	teamA=204;
	if(npc.Param[(125)]>0&&teamA==0)
	teamA=209;
	if(npc.Param[(126)]>0&&teamA==0)
	teamA=207;
	if(npc.Param[(127)]>0&&teamA==0)
	teamA=206;
	if(npc.Param[(128)]>0&&teamA==0)
	teamA=212;
	if(npc.Param[(129)]>0&&teamA==0)
	teamA=208;
	if(npc.Param[(125)]>0&&teamA!=0)
	teamB=209;
	if(npc.Param[(126)]>0&&teamA!=0)
	teamB=207;
	if(npc.Param[(127)]>0&&teamA!=0)
	teamB=206;
	if(npc.Param[(128)]>0&&teamA!=0)
	teamB=212;
	if(npc.Param[(129)]>0&&teamA!=0)
	teamB=208;
	array<Critter@>players;
	int num=GetAllPlayers(players);
	for(int i=0;i<num;i++)
	{
		if(players[i].ParamBase[(89)]==teamA&&players[i].IsPlayer())
		teamACount++;
		if(players[i].ParamBase[(89)]==teamB&&players[i].IsPlayer())
		teamBCount++;
		
	}
	uint enemy_team=0;
	uint my_team=0;
	if(teamA==204)
	{
		enemy_team=teamBCount;
		my_team=teamACount;
	}
	if(teamB==204)
	{
		enemy_team=teamA;
		my_team=teamBCount;
	}
	if(my_team>enemy_team+1)
	return false;
	return true;
}
