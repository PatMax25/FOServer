uint e_SwitchGate(array<uint>@values)
{
	Item@item=GetItem(values[0]);
	if(!(@item!=null))
	return 0;
	if((((item.LockerCondition)&((0x01)))!=0)&&IsNight())
	{
		
		uint16 hx=0,hy=0;
		Map@map=item.GetMapPosition(hx,hy);
		Critter@cr=map.GetCritter(hx,hy);
		if((@cr!=null))
		{
			if(values[1]<5)
			cr.MoveRandom();
			else
			cr.ToDead((103),null);
			values[1]++;
			return((1)*__TimeMultiplier);
		}
		
		if(item.LockerClose())
		{
			values[1]=0;
			return((1)*__TimeMultiplier*60);
		}
		else
		return((5)*__TimeMultiplier);
	}
	
	if(!(((item.LockerCondition)&((0x01)))!=0)&&!IsNight())
	{
		
		if(item.LockerOpen())
		{
			values[1]=0;
			return((1)*__TimeMultiplier*60);
		}
		else
		return((5)*__TimeMultiplier);
	} 
	
	return((5)*__TimeMultiplier);
}
