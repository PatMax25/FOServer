			case 13: mob.ParamBase[PE_DERMAL_IMPACT_ENH] += 1; break;
			case 14: mob.ParamBase[PE_PHOENIX_IMPLANTS] += 1; break;
			case 15: mob.ParamBase[PE_PHOENIX_IMPLANTS_ENH] += 1; break;
			case 16: mob.ParamBase[PE_EMP_IMPLANT] += 1; break;	
			}
			
			
			}
		
		
		}
	
