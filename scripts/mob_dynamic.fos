		RemoveItems(boss);
		if(!reno) SetArmor(boss);  else giverenores(boss, true, pid);
		boss.ParamBase[ST_PERCEPTION] = 20;
		if(!reno) SetBoss(boss); else giverenogun(boss, true, pid);
		rush(boss);
		}
