	mob.ParamBase[PE_QUICK_RECOVERY] = Random(0,1);
	mob.ParamBase[PE_SHARPSHOOTER] = Random(0,1);
	mob.ParamBase[PE_STONEWALL] = Random(0,1);
	mob.ParamBase[PE_MEDIC] = Random(0,1);
	int bt = mob.Param[ST_BODY_TYPE];
	switch(bt)
	{
		case BT_MEN:
		case BT_WOMEN:
		case BT_CHILDREN:
		case BT_GHOUL:
