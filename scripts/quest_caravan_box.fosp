bool _OnUseCaravan(Critter&cr,Scenery&scen,int skill,Item@item)
{
	if(skill!=-1)
	return false;
	
	if((@cr!=null))
	{
		Critter@npc=cr.GetMap().GetNpc((50),(0x0F)|(0x20),0);
		
		if((@npc!=null)&&GetUvar((6500),npc.Id,cr.Id)==(1))
		{
			if(GetUvar((6501),npc.Id,cr.Id)==0)
			{
				cr.Say((11),"You've completed the task assigned, speak to the caravan packer.");
				return false;
			}
			
			if(cr.CountItem((9626))>0)
			{
				cr.Say((11),"You've already picked up a box, you can't carry two.");
				return false;
			}
			
			cr.Say((11),"You've picked up a box from the caravan cart");
			Item@item=cr.AddItem((9626),1);
			if((@item!=null))
			item.SetEvent((5),"quest_caravan_box@_OnDropBox");
		}
		else
		{
			if((@npc!=null))
			npc.Say((4),"Don't touch that!");
			else
			cr.Say((11),"Don't touch that!");
			return false;
		}
	}
	return true;
}
