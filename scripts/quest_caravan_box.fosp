{
	return(GetUvar((6500),npc.Id,player.Id)==(0));
}

bool d_IsDone(Critter&player,Critter@npc)
{
	if(GetUvar((6500),npc.Id,player.Id)!=(1))
	return false;
	
	return(GetUvar((6501),npc.Id,player.Id)==0);
}

void r_StartQuest(Critter&player,Critter@npc)
{
	SetUvar((6500),npc.Id,player.Id,(1));
	SetUvar((6501),npc.Id,player.Id,10);
}

void r_CompleteQuest(Critter&player,Critter@npc)
{
	player.AddItem((41),250);
	player.ParamBase[(76)]+=300*(GetGvar((62001)));
	LogQuestExperience(player,50,"caravan_box");
	AddReputation(player,ReputationIndex(player.GetMap().GetLocation().GetProtoId()),Random(5,10),(300));
	SetUvar((6500),npc.Id,player.Id,(0));
}

void _OnDropBox(Item&item,Critter&cr)
{
	Map@map=cr.GetMap();
	
	Critter@npc=GetCaravanOfficer(map);
	if(!(@npc!=null))
	{
		DeleteItem(item);
		return;
	}
	
	array<Entire>entires;
	ParseEntires(map,entires,(110));
	for(uint i=0;i<entires.length();i++)
	{
		if((entires[i].HexX==item.HexX)&&(entires[i].HexY==item.HexY))
		{
			GameVar@var=GetUnicumVar((6501),npc.Id,cr.Id);
			if((@var!=null))
			var-=1;
			if(var<0)
			var=0;
			
			{;};
			DeleteItem(item);
			return;
		}
	}
	if(cr.GetMapId()==npc.GetMapId())
	{
		npc.Say((4),"You idiot, you can't drop it there!");
	}
	DeleteItem(item);
}

