				case(200):if(player.ParamBase[(200)]>=300+player.ParamBase[(1109)]+player.ParamBase[(4)])skillcap=true;break;
				case(201):if(player.ParamBase[(201)]>=300+player.ParamBase[(1110)]+player.ParamBase[(4)])skillcap=true;break;
