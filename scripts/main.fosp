					for(uint b=0;b<2;b++)
					{
						InjureCritter(cr,(Random(10,20)),(3),0,cr.Param[(188)]);
						cr.ParamBase[(120)]--;
					}
				}
				if(!(!(@cr!=null)||cr.IsDead()||
				(cr.IsNpc()&&cr.GetPlanes((1),null)==0)||
				(cr.IsPlayer()&&(cr.Param[(515)]!=0))))
				{
					continueBattle=true;
					break;
				}
			}
		}
		
		if(!continueBattle)
		map.EndTurnBased();
	}
} 

