			needPoints=4;
			else if(skillVal>175)
			needPoints=3;
			else if(skillVal>150)
			needPoints=2;
			
			if(player.ParamBase[(78)]<needPoints)
			break;
			
			skillVal++;   
			
			player.ParamBase[skillIndex]=skillVal;
			player.ParamBase[(78)]-=needPoints;
		}
	}
	else if(perkIndex>=(__PerkBegin)&&perkIndex<=(__PerkEnd))
