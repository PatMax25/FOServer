import string GetNick(uint gender,uint index,bool genderNick)from"npc_names";
import string GetSurname(uint index)from"npc_names";  

import void LogNames(bool printNames)from"npc_names";                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   

import int stringReplaceText(string&s,string@f,string@t)from"config_file";

Item@DogTag(Critter@cr)
{
	if((@cr!=null))
	return(DogTag(cr,cr));
	else
	return(null);
}

Item@DogTag(Critter@holder,Critter@target)
{
	if(!(@holder!=null))
	return(null);
	
	Item@dogtag=holder.AddItem((9601),1);
	if(!(@dogtag!=null))
	return(null);
	
	string@owner=null;
	string@info=null;
	
	uint gender=(Random(0,1)==0?(0):(1));
	uint year=__Year-Random(14,80);
	uint month=Random(1,12);
	string blood=GetBloodRandom();
	uint id=0;
	
	if((@target!=null))
	{
		year=GetLvar(target,(9002));
		month=GetLvar(target,(9004));
		blood=GetBloodType(target);
		if(target.Param[(71)]==(0)||target.Param[(71)]==(1))
		gender=target.Param[(71)];
		else
		{
			DeleteItem(dogtag);
			return(null);
		}
		
		if(target.IsPlayer())
		{
			id=target.Id;
			@owner=""+target.Name;
		}
		else
		id=target.Id-(5000000);
		
		if(IsSoldier(target)||IsRanger(target))
		{
			(dogtag.Val6)=(1);
			if(target.IsNpc())
			@owner="$SURNAME, $NAME";
			@info="$ID/$MOB/$YOB/$BLOOD";
			
			if(IsSoldier(target)||IsRanger(target))
			{
				(dogtag.Val5)=target.Param[(106)];
			}
		}
		else
		{
			(dogtag.Val6)=(2);
			if(target.IsNpc())
			@owner="$NAME $NICK_$SURNAME";
			@info="$MOB/$YOB/$BLOOD";
		}
	}
	else
	{
		if(Random(1,10)==10)
		(dogtag.Val6)=(0);
		else
		{
			id=Random(100,65535);
			
			if(Random(1,100)<=15)
			{
				(dogtag.Val6)=(1);
				@owner="$SURNAME, $NAME";
				@info="$ID/$MOB/$YOB/$BLOOD";
				
				if(Random(1,100)<=7)
				{
					array<uint8>factions={(2),(3),(5),(18)};
					(dogtag.Val5)=factions[Random(0,factions.length()-1)];
				}
			}
			else
			{
				(dogtag.Val6)=(2);
				@owner="$NAME $NICK_$SURNAME";
				@info="$MOB/$YOB/$BLOOD";
			}
		}
	}
	
	if((@owner!=null))
	{
		if((@target!=null)&&target.IsNpc())
		{
			GameVar@first=GetLocalVar((9005),target.Id);
			GameVar@gnick=GetLocalVar((9007),target.Id);
			GameVar@nick=GetLocalVar((9006),target.Id);
			GameVar@surname=GetLocalVar((9008),target.Id);
			
			if(first!=-1)
			stringReplaceText(owner,"$NAME",GetFirstName(target.Param[(71)],first.GetValue()));
			if(nick!=-1)
			stringReplaceText(owner,"$NICK_",GetNick(target.Param[(71)],nick.GetValue(),(gnick.GetValue()==1?true:false))+" ");
			if(surname!=-1)
			stringReplaceText(owner,"$SURNAME",GetSurname(surname.GetValue()));
		}
		
		stringReplaceText(owner,"$NAME",GetRandomFirstName(gender));
		if(Random(1,100)<=(7))
		stringReplaceText(owner,"$NICK_",GetRandomNick(gender)+" ");
		else
		stringReplaceText(owner,"$NICK_","");
		stringReplaceText(owner,"$SURNAME",GetRandomSurname());
		
		string picture="";
		switch((dogtag.Val5))
		{
			case(2):
			picture="tag_bos.png";
			break;
			case(3):
			picture="tag_enclave.png";
			break;
			case(5):
			picture="tag_ncr.png";
			break;
			case(18):
			picture="tag_rangers.png";
			break;
		}
		if(picture.length()>0)
		{
			dogtag.PicInv=GetStrHash("art\\inven\\"+picture);
			dogtag.Update();
		}
	}
	if((@info!=null))
	{
		stringReplaceText(info,"$ID",""+(id==0?"(unreadable)":""+id));
		stringReplaceText(info,"$MOB",""+(month<10?"0":"")+month);
		stringReplaceText(info,"$YOB",""+year);
		stringReplaceText(info,"$BLOOD",blood);
	}
	
	if((@owner!=null)||(@info!=null))
	{
		SetLexem(dogtag,"$owner",((@owner!=null)?owner:""));
		SetLexem(dogtag,"$info",((@info!=null)?info:""));
	}
	
	dogtag.Update();
	return(dogtag);
}

Item@DogTagBlank(Critter@holder)
{
	Item@dogtag=holder.AddItem((9601),1);
	if((@dogtag!=null))
	{
		(dogtag.Val6)=(0);
		return(dogtag);
	}
	else
	return(null);
}  

bool IsSoldier(Critter@cr)
{
	if(!(@cr!=null))
	return(false);
	
	if(cr.Param[(106)]==(3)||
	cr.Param[(106)]==(5)||
	cr.Param[(106)]==(2))
	{ 
		
		return(true);
	}
	
	return(false);
}

bool IsRanger(Critter@cr)
{
	if(!(@cr!=null))
	return(false);
	
	if(cr.Param[(106)]==(18))
	{
		
		return(true);
	}
	
	return(false);
}  

bool d_HasFactionDogtags(Critter&player,Critter@npc,int faction,int count)
{
	array<Item@>items;
	int have=player.GetItems(-1,items);
	if(have<count)
	return(false);
	
	have=0;
	for(uint i=0;i<items.length();i++)
	{
		if(items[i].GetProtoId()==(9601)&&
		(items[i].Val5)==faction)
		{
			have++;
			
			if(have>=count)
			return(true);
		}
	}
	
	return(false);
}

void r_RemoveFactionDogtags(Critter&player,Critter@npc,int faction,int count)
{
	array<Item@>items;
	player.GetItems(-1,items);
	
	int removed=0;
	for(uint i=0;i<items.length();i++)
	{
		if(items[i].GetProtoId()==(9601)&&
		(items[i].Val5)==faction)
		{
			removed++;
			DeleteItem(items[i]);
			
			if(removed>=count)
			return;
		}
	}
}                            

