void _CritterArenaIn(Map&map,Critter&cr)
{
	if(GetLvar(cr,(4212))==0)
	{
		TransferItemsToContOnMap(cr,GetMapByPid((227),0),(30),(128),(1));
		SetLvar(cr,(4212),1);
	}
}

void _CritterArenaOut(Map&map,Critter&cr)
{
	if(cr.IsLife()&&GetLvar(cr,(4212))==1)
	{
		TransferItemsFromContOnMap(cr,GetMapByPid((227),0),(30),(128));
		SetLvar(cr,(4212),0);
	}
}

void _CritterOut(Map&map,Critter&cr)
{
	if(cr.IsPlayer()&&(!(cr.GetSocket()==-1))&&GetLocalVar((7322),cr.Id)==0&&cr.Param[(77)]>1)
	{
		StartFirstTentQuest(cr);
	}
}
