class ItemInstance
{
	uint Pid;
	uint Chance;
	uint Min;
	uint Max;
	ItemInstance(uint16 pid,uint chance,uint min,uint max)
	{
		Pid=pid;
		Chance=chance;
		Min=min;
		Max=max;
	}
};

class ItemSpawner:IItemSpawner
{
	array<ItemInstance@>items;
	uint SumChance;
	uint SpawnTimeMin;
	uint SpawnTimeMax;
	uint16 LockerMin;
	uint16 LockerMax;
	bool Separate;
	bool Bonus;
	bool BonusLow;
	bool BonusOrNot;
	bool ClearAtSpawn;
	
	ItemSpawner()
	{
		SumChance=0;
		SpawnTimeMin=0;
		SpawnTimeMax=0;
		LockerMin=0;
		LockerMax=0;
		Separate=false;
		Bonus=false;
		BonusLow=false;
		BonusOrNot=false;
		ClearAtSpawn=false;
	}
	
	uint GetSpawnTime()
	{
		return((Random(SpawnTimeMin,SpawnTimeMax))*__TimeMultiplier*60);
	}
	uint GetLock()
	{
		int lockVal=Random(LockerMin,LockerMax);
		lockVal=(((lockVal)>((210)))?((210)):(((lockVal)<(0))?(0):(lockVal)));
		return lockVal;
	}
	IItemSpawner@Lock(uint16 min,uint16 max)
	{
		LockerMin=min;
		LockerMax=(((LockerMin)>(max))?(LockerMin):(max));
		return this;
	}
	IItemSpawner@SpawnTime(uint min,uint max)
	{
		SpawnTimeMin=min;
		SpawnTimeMax=(((SpawnTimeMin)>(max))?(SpawnTimeMin):(max));
		return this;
	}
	IItemSpawner@AddItem(uint16 pid,uint chance,uint min,uint max)
	{
		ItemInstance@item=@ItemInstance(pid,chance,min,max);
		items.insertLast(item);
		SumChance+=chance;
		return this;
	}
	IItemSpawner@AddItem(uint16 pid,uint chance)
	{
		return AddItem(pid,chance,1,1);
	}
	
	IItemSpawner@SetSeparate(bool separate)
	{
		Separate=separate;
		return this;
	}
	
	IItemSpawner@SetBonus(bool bonus)
	{
		Bonus=bonus;
		return this;
	}
	
	IItemSpawner@SetBonusLow(bool bonusLow)
	{
		BonusLow=bonusLow;
		return this;
	}
	
	IItemSpawner@SetBonusOrNot(bool bonusornot)
	{
		BonusOrNot=bonusornot;
		return this;
	}
	
	IItemSpawner@SetClearAtSpawn(bool clearSpawn)
	{
		ClearAtSpawn=clearSpawn;
		return this;
	}
	
	IItemSpawner@Clear()
	{
		SumChance=0;
		SpawnTimeMin=0;
		SpawnTimeMax=0;
		LockerMin=0;
		LockerMax=0;
		items.resize(0);
		Separate=false;
		Bonus=false;
		BonusLow=false;
		BonusOrNot=false;
		ClearAtSpawn=false;
		return this;
	}
	uint Spawn(Item&item)
	{
		array<Item@>spawneditems;
		uint HownManyItems=item.GetItems(uint(-1),spawneditems);  
		
		if(HownManyItems>=(5))
		{
			uint8 removed=0;
			
			for(uint i=0;i<spawneditems.length();i++)
			{
				if((@spawneditems[i]!=null))
				{
					if(Random(1,7)==1&&!IsSpecialItem(spawneditems[i]))
					{
						
						DeleteItem(spawneditems[i]);
						removed++;
					}
				}
			}
			HownManyItems=HownManyItems-removed;
			
		} 
		
		if(HownManyItems<(5)||!IsPublic(GetMap(item.MapId)))
		{
			if(ClearAtSpawn)
			{ 
				
				ClearContainerSoft(item);
			}
			
			if(!Separate)
			{
				uint num=Random(1,SumChance);
				uint idx=0;
				while(num>items[idx].Chance)
				num-=items[idx++].Chance;
				ItemInstance@spawnitem=items[idx];
				
				Item@it=item.AddItem(spawnitem.Pid,Random(spawnitem.Min,spawnitem.Max),0);
				
				if(Bonus)
				AddSpecialBonus(it);
				
				else if(BonusLow)
				AddSpecialBonusLow(it);
				
				else if(BonusOrNot)
				AddBonusOrNot(it);
				
				if(item.Val4==(3)||item.Val4==(1)||item.Val4==(2))
				SetDeterioration(it); 
				
			}
			else
			{
				for(uint i=0,j=items.length();i<j;i++)
				{
					ItemInstance@spawnitem=items[i];
					if(Random(1,100)<=int(spawnitem.Chance))
					{
						Item@it=item.AddItem(spawnitem.Pid,Random(spawnitem.Min,spawnitem.Max),0);
						
						if(Bonus)
						AddSpecialBonus(it);
						else
						if(BonusLow)
						AddSpecialBonusLow(it);
						else
						if(BonusOrNot)
						AddBonusOrNot(it);
						
						if(item.Val4==(3)||item.Val4==(1)||item.Val4==(2))
						SetDeterioration(it);
						
					}
				}
			}
		} 
		
		if(LockerMax!=0)
		{
			if((((item.LockerCondition)&((0x01)))!=0))
			LockerClose(item);
			if(IsLockableContainer(item.GetProtoId()))
			{
				item.LockerId=Random(0,0x7fffffff);
				item.LockerComplexity=GetLock();
			}
			else
			{
				item.LockerId=0;
				item.LockerComplexity=0;
			}
		}
		
		return GetSpawnTime();
	}
	
	uint GetValue()
	{
		uint sum=0;
		if(Separate)
		{
			
			for(uint i=0,j=items.length();i<j;i++)
			{
				ItemInstance@item=items[i];
				ProtoItem@proto=GetProtoItem(item.Pid);
				if(!(@proto!=null))
				{
					
					continue;
				}
				sum+=(((100)<(item.Chance))?(100):(item.Chance))*(item.Min+item.Max);
			}
			return sum/200;
		}
		else
		{
			for(uint i=0,j=items.length();i<j;i++)
			{
				ItemInstance@item=items[i];
				ProtoItem@proto=GetProtoItem(item.Pid);
				if(!(@proto!=null))
				{
					
					continue;
				}
				sum+=item.Chance*(item.Min+item.Max);
			}
		}
		return sum/(2*SumChance);
	}
	
	void LogValue(string&st)
	{
		
	}
};

