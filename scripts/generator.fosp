bool s_Generator(Critter&player,Scenery&terminal,int skill,Item@item)
{
	int bonus=0;
	if((@item!=null))
	{
		if(item.GetProtoId()==(75))
		bonus=25;
		else if(item.GetProtoId()==(308))
		bonus=50;
		else
		return false;
	}
	else if(skill!=(213))
	return false;
	
	player.Say((11),"Not implemented.");
	return true;
}
