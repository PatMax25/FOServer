	{
		this.id=id;
		return this;
	}
	
	IBrahminPen@TeamID(uint teamid)
	{
		this.teamid=teamid;
		return this;
	}
	
	IBrahminPen@SetOwner(Critter&owner)
	{
		if(!(@owner!=null))
		{
			
		}
		
		@this.owner=owner;
		return this;
	}
	
	IBrahminPen@MaxAmount(uint amount)
	{
		this.max=amount;
		return this;
	}
	
	IBrahminPen@AddBrahmins(uint amount)
	{
		if(initialized)
		return this;
		
		WLog("brahmin","Adding "+amount+" brahmin to pen");
		
		for(uint i=0;i<amount;i++)
		AddBrahmin();
		return this;
	}
	
};    

array<IBrahminPen@>BrahminPens;

IBrahminPen@GetBrahminPen(uint id)
{
	for(uint i=0;i<uint(BrahminPens.length());i++)
	{
		if(id==BrahminPens[i].GetID())
		{
			return BrahminPens[i];
		}
	}
	return null;
}

void AddBrahminPen(CBrahminPen&pen)
{
	BrahminPens.insertLast(pen);
}

void SaveBrahminPenData()
{
	for(uint i=0;i<BrahminPens.length();i++)
	BrahminPens[i].SaveData();
}

