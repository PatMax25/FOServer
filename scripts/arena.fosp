		name="error: "+i+","+player.Stat[(90)+i];
		text+=("$fvf"+i+name);
	}
}

string GetWeaponRuleString(uint weaponrule)
{
	switch(weaponrule)
	{
		case(0):
		return"All weapons";
		case(2):
		return"Melee only";
		case(1):
		return"Unarmed only";
		default:
		return"Error";
	}
	return"Error";
}

string GetArmorRuleString(uint armorrule)
