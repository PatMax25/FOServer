	if(!((@text!=null)&&text.length()==0))
	return 0;
	
	Contract@contract=FindContract(Pending,player.Id);
	if(!(@contract!=null))
	{
		return(19);
	}
	text+="$rules"+"Weapons: "+GetWeaponRuleString(contract.Rules[(0)])+" Allowed Armor: "+GetArmorRuleString(contract.Rules[(1)]);
