bool d_have_perk(Critter& player, Critter@ npc,int perk)
{
	uint type = SLOT_HAND1;
	if(perk<8)
		type = SLOT_ARMOR;
	Item@ item = player.GetItem(0, type);
	if(!valid(item))
		return false;
		
	switch(perk)
	{
	case 1: if(item.Val0 >= 15000) return true; break;
	case 2: if(item.Val0 >= 100000) return true; break;
	case 3: if(item.Val0 >= 200000) return true; break;
	case 4: if(item.Val0 >= 1250000) return true; break;
	case 5: if(item.Val0 >= 6000000) return true; break;
	
	case 8: if(item.Val0 >= 10000) return true; break;
	case 9: if(item.Val0 >= 75000) return true; break;
	case 10: if(item.Val0 >= 150000) return true; break;
	case 11: if(item.Val0 >= 1000000) return true; break;
	case 12: if(item.Val0 >= 5000000) return true; break;
	case 6:
	case 13: if(item.Val0 >= 50000000) return true; break;
	case 7:
	case 14: if(item.Val3 > 0) return true; break;
	}
	return false;
}

	
