bool d_wearing_weapon(Critter& player, Critter@ npc)
{
	Item@ item = player.GetItem(0, SLOT_HAND1);
	if(!valid(item))
		return false;
		
	if(item.IsStackable())
	{
	player.Say(SAY_NETMSG, "Can't upgrade this item");
	return false;
	}
    if(item.GetType() != ITEM_TYPE_WEAPON)
	{
	player.Say(SAY_NETMSG, "Can't upgrade this item");
	return false;
	}
	return true;
}

