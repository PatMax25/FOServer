void unsafe_Flag(Critter&player,int,int hexX,int hexY,string@param3,array<int>@param4)
{
	Map@map=player.GetMap();
	if(!(@map!=null))
	return;
	int cdLeader=180-player.Param[(3)]*5;
	uint deletecd=320;
	if(player.Timeout[(242)]>0)
	{
		player.Say((11),"|-65536 Too Tired");
		return;
	}
	if(player.Param[(441)]==0)
	{
		player.Say((11),"|-65536 I'm not a Leader");
		return;
	}
	if(player.Param[(106)]<2)
	{
		player.Say((11),"|-65536 I'm not in a faction");
		return;
	}
	
	if(player.CountItem((286))>0)
	{
		Item@item=player.GetItem((286),-1);
		
		if(item.GetCount()>1)
		item.SetCount(item.GetCount()-1);
		else
		DeleteItem(item);
		player.Animate(0,(28),null,true,true);
		Item@flag=map.AddItem(player.HexX,player.HexY,9300,1);
		if(player.Param[(191)]==0)
		player.Say((11),"|0xCC0000 I didn't choose a bonus, To choose it, I should type in chat command |0xCCCCCC ~leader");
		flag.Val6=player.Param[(89)];
		flag.Val8=player.Param[(106)];
		flag.Val9=player.Param[(191)];
		flag.Val7=deletecd;
		
		if(player.Param[(89)]>203&&player.Param[(89)]<213)
		flag.ChangeProto(9000+player.Param[(89)]);
		else flag.ChangeProto(9200+player.Param[(106)]);
		
		flag.SetScript("followers_menu@Flag");
		player.TimeoutBase[(242)]=((__FullSecond)+(((cdLeader)*__TimeMultiplier))); 
		
	}
	else player.Say((2),"|-65536 I dont have any wood");
	
