			continue;
			
			for(uint j=0,l=Encounter.Groups.length();j<l;j++)
			{
				if(i==j)
				continue;
				CEncounterGroup@gr2;
				if(Encounter.Groups[j]<(146))
				@gr2=EncounterGroups[Encounter.Groups[j]];
				Actions.insertLast(i);
				Actions.insertLast(j);
