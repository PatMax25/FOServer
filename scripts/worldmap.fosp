CFoundedEncounter@CreateFoundedEncounter(uint x,uint y,CEncounter@encounter,CZone@zone,uint ownerId,bool ownerGroup)
{    
	
	for(int i=0,j=FoundedEncounters.length();i<j;i++)
	{
