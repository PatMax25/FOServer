	{
		for(uint i=0,j=Flags.length();i<j;i++)
		if(Flags[i]==flag)
		return true;
		return false;
	}  
	
	uint GetQuantity(uint group)
	{
		for(uint i=0,j=Groups.length();i<j;i++)
		if(Groups[i]==group)
		return EncounterGroups[Groups[i]].GetModifiedQuantity(Quantities[i]);
		return 0;
	}  
	
