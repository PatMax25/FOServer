	int DetermineAction(int gr1,int gr2)
	{
		int status=GetGroupsStatus(gr2,gr1);
		switch(status)
		{
			case(0):
			return(0);
			case(1):
			return(0);
			case(2):
			return(1);
		}
		return(1);
	}  
	
	void PrepareActions()
	{
		for(uint i=0,k=Encounter.Groups.length();i<k;i++)
		{
			CEncounterGroup@gr1;
			if(Encounter.Groups[i]<(146))
			@gr1=EncounterGroups[Encounter.Groups[i]];
