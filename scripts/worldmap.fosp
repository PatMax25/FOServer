uint GetFreeDir(array<bool>&dirBusy,uint8&inout firstDirIdx)
{
	
	uint[]dirs={0,2,1,0,2};
	if(firstDirIdx==0)
	firstDirIdx=Random(0,2);
	for(uint i=firstDirIdx;i<firstDirIdx+3;i++)
	{
		int dir=dirs[i];
		int opp=(dir+3)%6;
		
		if(dirBusy[dir]&&!dirBusy[opp])
		{
			
			dirBusy[opp]=true;
			return opp;
		}
		else if(!dirBusy[dir]&&!dirBusy[opp])
		{
			dirBusy[dir]=true;
			return dir;
		}
	} 
	
	int dir=Random(0,5);
	dirBusy[dir]=true;
	return dir;
}

