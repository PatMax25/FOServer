		(!(@ge.Encounter!=null)||(!ge.Encounter.Special&&CheckChecks(ge.Encounter.Checks,group)))&&
		uint(sqrt(float(((ge.PositionX-x)*(ge.PositionX-x))+((ge.PositionY-y)*(ge.PositionY-y)))))<=3*radius&&
		group[0].Id!=ge.OwnerId&&
