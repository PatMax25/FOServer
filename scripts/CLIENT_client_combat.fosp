	toHit=(((toHit)>(95+chosen.Param[(395)]+normaldmg))?(95+chosen.Param[(395)]+normaldmg):(((toHit)<(5+chosen.Param[(395)]))?(5+chosen.Param[(395)]):(toHit)));
	
	return toHit;
}

uint GetHitAim(int hitLocation)
{
	switch(hitLocation)
