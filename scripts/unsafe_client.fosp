			||items[i].GetProtoId()==(22114)||items[i].GetProtoId()==(22115)||items[i].GetProtoId()==(22116)
			||items[i].GetProtoId()==(22117))
			{
				items.removeAt(i);
				--i;
				--j;
			}
			else if(useFilter!=-1)
