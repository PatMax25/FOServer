			if((@cont!=null))
			target.Animate(0,(28),null,true,true);
			else if((@body!=null))
			target.Animate(0,(27),null,true,true);
		} 
		
		if((@cont!=null))
		target.ShowContainer(null,cont,(7));
		else if((@body!=null))
		target.ShowContainer(body,null,(4));
	}
	target.RunClientScript("client_interface@_EnableItemsMove",0,0,0,"",null);
