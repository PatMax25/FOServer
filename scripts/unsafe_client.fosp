{
	if(!target.IsKnockout()&&!target.IsDead()&&target.Param[(75)]>=0)
	{
		if(target.Param[(510)]!=0&&target.GetAccess()<(1))
		target.ParamBase[(510)]=0;
		
		Item@cont;
		Critter@body;
		
		if(contId>0)
		{
			@cont=target.GetMap().GetItem(contId);
