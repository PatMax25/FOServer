void overseer(Critter&overseer,bool firstTime)
{
	overseer.SetEvent((16),"mob_dynamic@_Alert");
	overseer.SetEvent((0),"mob_dynamic@_DontMove");
	overseer.SetEvent((2),"_OverSeerIsDead");
	if(firstTime)
	{
		SetWeapon(overseer,0);
		SetParatersToMob(overseer,overseer.GetMap(),true);
		
		uint8 S=1,P=1,E=1,C=1,I=1,A=1,L=1;
		for(uint8 i=1;i<33;i++)
		{
			switch(Random(1,7))
			{
				case 1:if(S==10)i--;else S++;break;
				case 2:if(P==10)i--;else P++;break;
				case 3:if(E==10)i--;else E++;break;
				case 4:if(C==10)i--;else C++;break;
				case 5:if(I==10)i--;else I++;break;
				case 6:if(A==10)i--;else A++;break;
				case 7:if(L==10)i--;else L++;break;
			}
		}
		
		overseer.ParamBase[(0)]=S;
		overseer.ParamBase[(1)]=P;
		overseer.ParamBase[(2)]=E;
		overseer.ParamBase[(3)]=C;
		overseer.ParamBase[(4)]=I;
		overseer.ParamBase[(5)]=A;
		overseer.ParamBase[(6)]=L;
		
		overseer.ParamBase[(77)]=20;
		overseer.ParamBase[(66)]=50000;
		overseer.ParamBase[(7)]=3000;
		overseer.ParamBase[(72)]=3000;
	}
	
	overseer.ParamBase[(511)]=1;
	overseer.ParamBase[(523)]=1;
	overseer.ParamBase[(532)]=1;
	overseer.ParamBase[(522)]=1;
	overseer.ParamBase[(541)]=1;
	overseer.ParamBase[(512)]=1;
	overseer.ParamBase[(524)]=0;  
	
	(overseer.ParamBase[(534)]=overseer.Param[(534)]|((0x00000004)));
}

bool _UseDoorConsole(Item&item,Critter&player,int skill)
{
	
	player.Animate((1),(28),item,true,true);
	
	if((skill==-1)||(skill==(209)))
	{
		Map@map=GetMap(item.MapId);
		Item@vaultdoor=map.GetItem(106,83,2173);  
		
		if((((vaultdoor.LockerCondition)&((0x01)))!=0))
		{
			player.Say((11),"You can't close those vault door manually.");
		}
		else
		{
			if(player.CountItem((25444))>0)
			player.Say((11),"Try with this unique vault key. It should fit to the keyhole.");         
			
			else
			player.Say((11),"Message from console: We can't open this vault from inside. Try find an access key.");
		}
		
		return true;
	} 
	
	return false;
}

bool _UseKeyOnConsole(Item&item,Critter&player,Item@usedItem)
{
	if(usedItem.GetProtoId()==(25444))
	{
		if(Random(1,99)>74)
		{
			player.Animate((1),(28),item,true,true);
			Map@map=GetMap(item.MapId);
			Item@vaultdoor=map.GetItem(106,83,2173);
			Item@vaultgate=map.GetItem(106,67,2008);
			array<uint>vaultdata={vaultdoor.Id,vaultgate.Id,map.Id};
			
			if((((vaultdoor.LockerCondition)&((0x01)))!=0))
			{
				player.Say((11),"You can't close those vault door manually. Even by vault key.");
			}
			else
			{
				
				ServerEventCNTSet("Vault's 13 outer door has been open. It will remain in this state for the next %COUNTDOWN% left.",(23),(11),(((20))*__TimeMultiplier*60));
				array<Item@>blockers; 
				
				CreateTimeEvent(((__FullSecond)+((((20))*__TimeMultiplier*60))),"EndV13Event",vaultdata,true);
				FillContainers(map);
				WriteROMToComputers(map);
				SwitchState(vaultdoor);
				SwitchState(vaultgate);
				
				array<Map@>maps;
				map.GetLocation().GetMaps(maps);
				for(uint8 i=0;i<maps.length();i++)
				{
					if(!(@maps[i]!=null))
					continue;
					uint reqNumMobs;
					
					if(maps[i].GetProtoId()==(92))
					{
						uint lookforseer=maps[i].GetNpcCount((203),(0x0F));
						
						for(uint8 j=0;j<lookforseer;j++)
						{
							Critter@OverSeer=maps[i].GetNpc((203),(0x0F),j);
							if(OverSeer.GetProtoId()==882||OverSeer.GetProtoId()==901)
							{
								if(OverSeer.IsDead())OverSeer.ToLife();
								OverSeer.ParamBase[(72)]=3000;
							}
						} 
						
						Item@forcefield=maps[i].GetItem(64,99,(9678));
						if(!(@forcefield!=null))maps[i].AddItem(64,99,(9678),1);
						@forcefield=map.GetItem(63,99,(820));
						if(!(@forcefield!=null))maps[i].AddItem(63,99,(820),1);
						@forcefield=map.GetItem(65,99,(820));
						if(!(@forcefield!=null))maps[i].AddItem(65,99,(820),1);
					}
					
					if(maps[i].GetProtoId()==(90))
					{
						uint lookformobs=maps[i].GetNpcCount((203),(0x03));
						uint realNum=0;
						
						for(uint8 j=0;j<lookformobs;j++)
						{
							Critter@dweller=maps[i].GetNpc((203),(0x03),j);
							if(dweller.GetProtoId()==898||dweller.GetProtoId()==899||dweller.GetProtoId()==900)
							{
								realNum++;
							}
						}
						reqNumMobs=15-realNum;
					}
					
					switch(maps[i].GetProtoId())
					{ 
						
						case(91):
						reqNumMobs=15-maps[i].GetNpcCount((203),(0x03));break;
						case(92):
						reqNumMobs=16-maps[i].GetNpcCount((203),(0x03));break;
					}
					
					FillMapByMobs(maps[i],reqNumMobs,(21),0,(7));           
					
				}
				DeleteItem(usedItem);
				player.Say((11),"Unfortunately, the key was destroyed.");
			}
			
			return true;
		}
		else
		{
			DeleteItem(usedItem);
			player.Say((11),"Sound like your card was fake, try with another.");
			return true;
		}
	}
	else
	{
		player.Say((11),"It doesn't fit into the keyhole.");
		return true;
	}
}

uint EndV13Event(array<uint>@values)
{
	Item@vaultdoor=GetItem(values[0]);
	Item@vaultgate=GetItem(values[1]);
	Map@vault=GetMap(values[2]);
	
	if(!(@vaultdoor!=null))
	return 0;
	
	if(!(@vaultgate!=null))
	return 0; 
	
	array<Critter@>critters;
	
	vault.GetCrittersHex(106,83,0,(0x03),critters);
	vault.GetCrittersHex(106,84,0,(0x03),critters);
	vault.GetCrittersHex(106,85,0,(0x03),critters);
	vault.GetCrittersHex(105,86,0,(0x03),critters);
	vault.GetCrittersHex(107,86,0,(0x03),critters);
	vault.GetCrittersHex(105,67,0,(0x03),critters);
	vault.GetCrittersHex(105,68,0,(0x03),critters);
	vault.GetCrittersHex(106,67,0,(0x03),critters);
	Map@map2=GetMap(102);
	Map@map3=GetMap(103);
	array<Critter@>critteres;
	uint num=map2.GetCritters(0,(0x01),critteres);
	for(uint i=0;i<num;i++)
	{
		critteres[i].ToDead((103),null);
	}
	uint num2=map3.GetCritters(0,(0x01),critteres);
	for(uint i=0;i<num2;i++)
	{
		critteres[i].ToDead((103),null);
	}
	
	for(uint i=0,j=critters.length();i<j;i++)
	{
		if((@critters[i]!=null))
		{
			critters[i].Say((11),"You were squeezed by vault door.");
			critters[i].ToDead((117),null);
		}
	}
	
	if((((vaultdoor.LockerCondition)&((0x01)))!=0))
	{
		LockerClose(vaultdoor); 
		
	}
	
	if((((vaultgate.LockerCondition)&((0x01)))!=0))
	{
		LockerClose(vaultgate); 
		
	}
	return 0;
}

void _OverSeerIsDead(Critter&overseer,Critter@killer)
{
	
	Map@map=overseer.GetMap();
	
	Item@forcefield=map.GetItem(64,99,(9678));
	if((@forcefield!=null))DeleteItem(forcefield);
	@forcefield=map.GetItem(63,99,(820));
	if((@forcefield!=null))DeleteItem(forcefield);
	@forcefield=map.GetItem(65,99,(820));
	if((@forcefield!=null))DeleteItem(forcefield); 
	
	overseer.ClearEnemyStack();
} 

void FillContainers(Map&map)
{
	if(!(@map!=null))
	return;
	
	Location@loc=map.GetLocation();
	array<Map@>maps;
	loc.GetMaps(maps);
	for(uint i=0,j=maps.length();i<j;i++)
	{
		if((@maps[i]!=null))
		{
			if(maps[i].GetProtoId()==(90))
			{
				array<Item@>lockers;
				maps[i].GetItemsByType((8),lockers);
				for(uint k=0,l=lockers.length;k<l;k++)
				{
					if(!(@lockers[k]!=null))
					return; 
					
					if((((lockers[k].LockerCondition)&((0x01)))!=0))
					LockerClose(lockers[k]);
					
					if(lockers[k].GetProtoId()!=(211))
					{
						lockers[k].AddItem(tier4ammo(),Random(1,20),0);
					}
				}
				
			}
			if(maps[i].GetProtoId()==(91))
			{
				array<Item@>lockers;
				maps[i].GetItemsByType((8),lockers);
				for(uint k=0,l=lockers.length;k<l;k++)
				{
					if(!(@lockers[k]!=null))
					return; 
					
					if((((lockers[k].LockerCondition)&((0x01)))!=0))
					LockerClose(lockers[k]);
					
					uint8 losuj=Random(1,10);
					
					if(losuj>=1&&losuj<=5)
					{
						Item@it=lockers[k].AddItem(tier3weapon(),1,0);
						AddSpecialBonusLow(it);
					}
					
					if(losuj>=6&&losuj<=9)
					{
						Item@it=lockers[k].AddItem(tier3armor(),1,0);
						AddSpecialBonus(it);
					}
					if(losuj==10)
					lockers[k].AddItem(tier4parts(),1,0);
					
				}
			}
			if(maps[i].GetProtoId()==(92))
			{
				array<Item@>lockers;
				maps[i].GetItemsByType((8),lockers);
				for(uint k=0,l=lockers.length;k<l;k++)
				{
					if(!(@lockers[k]!=null))
					return; 
					
					if((((lockers[k].LockerCondition)&((0x01)))!=0))
					LockerClose(lockers[k]);
					
					uint8 losuj=Random(1,20);
					
					if(losuj>=1&&losuj<=12)
					{
						Item@it=lockers[k].AddItem(tier3armor(),1,0);
						
					}
					
					if(losuj>=13&&losuj<=15)
					{
						Item@it=lockers[k].AddItem(tier4parts(),1,0);
						
					}
					if(losuj==20)
					lockers[k].AddItem(tier4bp(),1,0);
					
					if(losuj>=16&&losuj<=19)
					lockers[k].AddItem(implant(),1,0);
					
					losuj=Random(1,10);
					
					if(losuj>=1&&losuj<=7)
					lockers[k].AddItem(tier3ammo(),Random(10,50),0);
					
					if(losuj>=8&&losuj<=10)
					lockers[k].AddItem(tier4ammo(),Random(1,5),0);
					
				}
			}
			
		}
	}
	return;
	
}

void WriteROMToComputers(Map&map)
{
	if(!(@map!=null))
	return;
	
	Location@loc=map.GetLocation();
	array<Map@>maps;
	loc.GetMaps(maps);
	
	for(uint i=0,j=maps.length();i<j;i++)
	{
		if((@maps[i]!=null))
		{
			if(maps[i].GetProtoId()==(90))
			{
				array<Item@>computers;
				maps[i].GetItems((25447),computers);
				maps[i].GetItems((25448),computers);
				
				for(uint k=0,l=computers.length;k<l;k++)
				{
					if(!(@computers[k]!=null))
					return;
					
					computers[k].SetEvent((4),"_UseComputer1");
				}
			}
			
			if(maps[i].GetProtoId()==(91))
			{
				array<Item@>computers;
				maps[i].GetItems((25447),computers);
				maps[i].GetItems((25448),computers);
				
				for(uint k=0,l=computers.length;k<l;k++)
				{
					if(!(@computers[k]!=null))
					return;
					
					computers[k].SetEvent((4),"_UseComputer2");
				}
			}
			if(maps[i].GetProtoId()==(92))
			{
				array<Item@>computers;
				maps[i].GetItems((25447),computers);
				maps[i].GetItems((25448),computers); 
				
				for(uint k=0,l=computers.length;k<l;k++)
				{
					if(!(@computers[k]!=null))
					return;
					
					if(Random(1,20)==1)
					computers[k].SetEvent((4),"_UseComputer3Special");
					else
					computers[k].SetEvent((4),"_UseComputer3Regullar");
				}
			}
		}
		
	}
	
	return;
}

bool _UseComputer1(Item&item,Critter&player,int skill)
{
	player.Animate((1),(28),item,true,true);
	if((skill==-1)||(skill==(213))||(skill==(212)))
	{
		player.Say((11),"This computer machine seems to be broken. Maybe you should try one on lower level?");
		return true;
	}
	else
	return false;
