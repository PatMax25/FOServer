	ReloadWeapon(follower);
	
	Critter@master=GetMaster(follower);
	if(!(@master!=null))
	{
		follower.Wait((15000));
		return;
	}
	
	Map@map=follower.GetMap();
	if((@map!=null)&&map.GetLocation().IsEncounter()&&!IsOnSameMap(master,follower))
	{
