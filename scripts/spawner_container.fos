#define MAX_SPAWNED_ITEMS                    (5)         // config
#define MAX_COMPLEXITY                       (210)       // config
#define SPAWN_INTERVAL    # (_time)  REAL_MINUTE(_time)   // debug

import bool LockerClose(Item& item) from "lockers";
import void AddBonusOrNot(Item@ it) from "item_bonus";
import void AddSpecialBonus(Item@ it) from "item_bonus";
import void AddSpecialBonusLow(Item@ it) from "item_bonus";
import void ClearContainerSoft(Item@container) from "utils";
import bool IsLockableContainer(uint pid) from "lockers";
import void SetDeterioration(Item& item) from "utils";
import bool IsPublic(Map@ map) from "utils";


class ItemInstance
{
    uint Pid;
    uint Chance;
    uint Min;
    uint Max;
    ItemInstance(uint16 pid, uint chance, uint min, uint max)
    {
        Pid = pid;
        Chance = chance;
        Min = min;
        Max = max;
    }
};

class ItemSpawner : IItemSpawner
{
    array<ItemInstance@> items;
    uint                 SumChance;
    uint                 SpawnTimeMin;
    uint                 SpawnTimeMax;
    uint16               LockerMin;
    uint16               LockerMax;
    bool                 Separate;
    bool                 Bonus;
    bool                 BonusLow;
    bool                 BonusOrNot;
    bool                 ClearAtSpawn;

    ItemSpawner()
    {
        SumChance = 0;
        SpawnTimeMin = 0;
        SpawnTimeMax = 0;
        LockerMin = 0;
        LockerMax = 0;
        Separate = false;
        Bonus = false;
        BonusLow = false;
        BonusOrNot = false;
        ClearAtSpawn = false;
    }

    uint GetSpawnTime()
    {
        return SPAWN_INTERVAL(Random(SpawnTimeMin, SpawnTimeMax));
    }
    uint GetLock()
    {
        int lockVal = Random(LockerMin, LockerMax);
        lockVal = CLAMP(lockVal, 0, MAX_COMPLEXITY);
        return lockVal;
    }
    IItemSpawner@ Lock(uint16 min, uint16 max)
    {
        LockerMin = min;
        LockerMax = MAX(LockerMin, max);
        return this;
    }
    IItemSpawner@ SpawnTime(uint min, uint max)   // set in real minutes
    {
        SpawnTimeMin = min;
        SpawnTimeMax = MAX(SpawnTimeMin, max);
        return this;
    }
    IItemSpawner@ AddItem(uint16 pid, uint chance, uint min, uint max)
    {
        ItemInstance@ item = @ItemInstance(pid, chance, min, max);
        items.insertLast(item);
        SumChance += chance;
        return this;
    }
    IItemSpawner@ AddItem(uint16 pid, uint chance)
    {
        return AddItem(pid, chance, 1, 1);
    }

    IItemSpawner@ SetSeparate(bool separate)
    {
        Separate = separate;
        return this;
    }

    IItemSpawner@ SetBonus(bool bonus)
    {
        Bonus = bonus;
        return this;
    }

    IItemSpawner@ SetBonusLow(bool bonusLow)
    {
        BonusLow = bonusLow;
        return this;
    }

    IItemSpawner@ SetBonusOrNot(bool bonusornot)
    {
        BonusOrNot = bonusornot;
        return this;
    }

    IItemSpawner@ SetClearAtSpawn(bool clearSpawn)
    {
        ClearAtSpawn = clearSpawn;
        return this;
    }

    IItemSpawner@ Clear()
    {
        SumChance = 0;
        SpawnTimeMin = 0;
        SpawnTimeMax = 0;
        LockerMin = 0;
        LockerMax = 0;
        items.resize(0);
        Separate = false;
        Bonus = false;
        BonusLow = false;
        BonusOrNot = false;
        ClearAtSpawn = false;
        return this;
    }
    uint Spawn(Item& item)
    {
        array<Item@> spawneditems;
        uint HownManyItems = item.GetItems(uint(-1), spawneditems);

        //Rotate items when MAX_SPAWNED_ITEMS is reached, only in public locations

        if(HownManyItems >= MAX_SPAWNED_ITEMS)
        {
            uint8 removed = 0;
            //Log("Locker <" + item.Id + "> with Val4 <" + item.Val4 + "> has spawned " + spawneditems.length() + "/" + MAX_SPAWNED_ITEMS + " items inside.");
            for(uint i = 0; i < spawneditems.length(); i++)
            {
                if(valid(spawneditems[i]))
                {
                    if(Random(1, 7) == 1 && !IsSpecialItem(spawneditems[i]))
                    {
                        //Log("Removing item.Id <" + spawneditems[i].Id + ">, pid <" + spawneditems[i].GetProtoId() + "> from locker <" + item.Id + ">...");
                        DeleteItem(spawneditems[i]);
                        removed++;
                    }
                }
            }
            HownManyItems = HownManyItems - removed;
            //Log("Removed " + removed + " items from locker " + item.Id + ". " + HownManyItems + " items left...");
        }

        //Don't spawn another items when MAX_SPAWNED_ITEMS is reached, only in public locations
        if(HownManyItems < MAX_SPAWNED_ITEMS || !IsPublic(GetMap(item.MapId)))
        {
            if(ClearAtSpawn)
            {
                // See e_Spawn function - worldremaker
                // if(item.Val5>=7) ClearContainer(item);
                ClearContainerSoft(item);
            }

            if(!Separate)
            {
                uint num = Random(1, SumChance);
                uint idx = 0;
                while(num > items[idx].Chance)
                    num -= items[idx++].Chance;
                ItemInstance@ spawnitem = items[idx];

                Item@ it = item.AddItem(spawnitem.Pid, Random(spawnitem.Min, spawnitem.Max), 0);

                if(Bonus)
                    AddSpecialBonus(it);

                else if(BonusLow)
                    AddSpecialBonusLow(it);

                else if(BonusOrNot)
                    AddBonusOrNot(it);

                if(item.Val4 == FAT_TOWN_CONTAINER || item.Val4 == FAT_PROTECTED_TOWN_CONTAINER || item.Val4 == FAT_ENCOUNTER_STATIC_CONTAINER)
                    SetDeterioration(it);


            }
            else
            {
                for(uint i = 0, j = items.length(); i < j; i++)
                {
                    ItemInstance@ spawnitem = items[i];
                    if(Random(1, 100) <= int(spawnitem.Chance))
                    {
                        Item@ it = item.AddItem(spawnitem.Pid, Random(spawnitem.Min, spawnitem.Max), 0);

                        if(Bonus)
                            AddSpecialBonus(it);
                        else
                        if(BonusLow)
                            AddSpecialBonusLow(it);
                        else
                        if(BonusOrNot)
                            AddBonusOrNot(it);

                        if(item.Val4 == FAT_TOWN_CONTAINER || item.Val4 == FAT_PROTECTED_TOWN_CONTAINER || item.Val4 == FAT_ENCOUNTER_STATIC_CONTAINER)
                            SetDeterioration(it);

                    }
                }
            }
        }

            //Lock the locker when MAX_SPAWNED_ITEMS is reached anyway.
            if(LockerMax != 0)
            {
                if(FLAG(item.LockerCondition, LOCKER_ISOPEN))
                    LockerClose(item);
                if(IsLockableContainer(item.GetProtoId()))
                {
                    item.LockerId = Random(0, 0x7fffffff);
                    item.LockerComplexity = GetLock();
                }
                else
                {
                    item.LockerId = 0;
                    item.LockerComplexity = 0;
                }
            }

        return GetSpawnTime();
    }

    uint GetValue()
    {
        uint sum = 0;
        if(Separate)
        {

            for(uint i = 0, j = items.length(); i < j; i++)
            {
                ItemInstance@ item = items[i];
                ProtoItem@    proto = GetProtoItem(item.Pid);
                if(!valid(proto))
                {
                 //   Log("ERR: Spawner uses invalid proto: " + item.Pid);
                    continue;
                }
                sum += MIN(100, item.Chance) * (item.Min + item.Max);
            }
            return sum / 200;
        }
        else
        {
            for(uint i = 0, j = items.length(); i < j; i++)
            {
                ItemInstance@ item = items[i];
                ProtoItem@    proto = GetProtoItem(item.Pid);
                if(!valid(proto))
                {
                  //  Log("ERR: Spawner uses invalid proto: " + item.Pid);
                    continue;
                }
                sum += item.Chance * (item.Min + item.Max);
            }
        }
        return sum / (2 * SumChance);
    }

    void LogValue(string& st)
    {
      //  WLog("spawner_container", st + " spawner value: " + (SpawnTimeMax == 0 ? (GetValue() + " once.") : ((GetValue() * (2880 / (SpawnTimeMin + SpawnTimeMax))) + " daily")), DEBUG_MESSAGE);
    }
};

