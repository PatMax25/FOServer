uint[]CaveLocations=
{
	(140),
	(141),
	(142),
	(143),
	(144),
	(145),
	(146)
};

uint[]find_pids={(78),(89),(104),(112)};
uint[]find_nodes={100,200,300,400};

int[]reward_bags={80,90,100,110,430,431,432};

bool d_None(Critter&player,Critter@npc)
{
	return(GetUvar((20010),npc.Id,player.Id)==(0));
}

bool d_CanAsk(Critter&player,Critter@npc)
{
	return(GetUvar((20010),npc.Id,player.Id)>0);
}

bool d_Briefed(Critter&player,Critter@npc)
{
	return(GetUvar((20010),npc.Id,player.Id)==(1));
}

bool d_OnGoing(Critter&player,Critter@npc)
{
	return(GetUvar((20010),npc.Id,player.Id)==(2));
}

bool d_Completed(Critter&player,Critter@npc)
{
	return(GetUvar((20010),npc.Id,player.Id)==(3));
}

bool d_NotCompleted(Critter&player,Critter@npc)
{
	return(GetUvar((20010),npc.Id,player.Id)!=(3));
}

bool d_HasStuff(Critter&player,Critter@npc)
{
	if(GetUvar((20010),npc.Id,player.Id)!=(2))
	return false;
	
	uint pid=GetUvar((20011),npc.Id,player.Id);
	Item@item=player.GetItem(pid,-1);
	if(!(@item!=null))
	return false;
	return(uint(item.Val1)==player.Id);
}

void r_ReceiveReward(Critter&player,Critter@npc)
{
	player.AddItem((41),Random(200,500));
	GiveBag((reward_bags[Random(0,reward_bags.length()-1)]),player);
}

void r_CompleteQuest(Critter&player,Critter@npc)
{
	SetUvar((20010),npc.Id,player.Id,(3));
	uint pid=GetUvar((20011),npc.Id,player.Id);
	player.DeleteItem(pid,1);
	player.ParamBase[(76)]+=500*(GetGvar((62001)));
	LogQuestExperience(player,500*(GetGvar((62001))),"rq_cave");
	
	AddReputation(player,ReputationIndex(player.GetMap().GetLocation().GetProtoId()),75,(500));
	
	Location@loc=GetLocation(GetUvar((20013),npc.Id,player.Id));
	if((@loc!=null))
	{
		{;};
		DeleteLocation(loc.Id);
	}
	else
	{;};
}

void r_AcceptQuest(Critter&player,Critter@npc)
{
	SetUvar((20010),npc.Id,player.Id,(2));
	
	uint locid=SpawnLoc(player,npc);
	SetUvar((20013),npc.Id,player.Id,locid);
	
	Location@loc=GetLocation(locid);
	
	Map@map=loc.GetMapByIndex(1);
	if(!(@map!=null))
	{
		Log("ERR: Can't find cave map of quest location");
		return;
	}
	map.SetData((21),GetUvar((20011),npc.Id,player.Id));
	map.SetData((22),player.Id);
}

uint r_ShowQuest(Critter&player,Critter@npc)
{
	return GetUvar((20012),npc.Id,player.Id);
} 

uint r_GenerateQuest(Critter&player,Critter@npc)
{
	uint pid=0;
	uint node=0;
	
	node=Random(0,find_nodes.length()-1);
	pid=find_pids[node];
	int force=find_nodes[node];
	
	SetUvar((20010),npc.Id,player.Id,(1));
	SetUvar((20011),npc.Id,player.Id,pid);
	SetUvar((20012),npc.Id,player.Id,node);
	return force;
}

uint SpawnLoc(Critter@player,Critter&npc)
{
	double dist=double(Random(50,120));
	uint x=npc.WorldX+int(dist*sin(double(Random(0,359))*(0.01745329251994329576923690768488)));
	uint y=npc.WorldY+int(dist*cos(double(Random(0,359))*(0.01745329251994329576923690768488)));
	
	Critter@[]crits={player};
	int loc=CreateLocation((CaveLocations[Random(0,CaveLocations.length()-1)]),x,y,crits);
	if(player.Param[(516)]==(2))
	SetTurnBasedAvailability(GetLocation(loc)); 
	
	return loc;
}
